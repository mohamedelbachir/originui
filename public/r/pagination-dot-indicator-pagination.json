      "content": "import {\r\r\n  Pagination,\r\r\n  PaginationContent,\r\r\n  PaginationItem,\r\r\n  PaginationLink,\r\r\n} from \"@/registry/default/ui/pagination\";\r\r\nimport { ChevronLeft, ChevronRight } from \"lucide-react\";\r\r\n\r\r\nexport default function DotIndicatorPagination() {\r\r\n  return (\r\r\n    <div className=\"p-6\">\r\r\n      <Pagination>\r\r\n        <PaginationContent className=\"gap-1\">\r\r\n          <PaginationItem>\r\r\n            <PaginationLink\r\r\n              href=\"#\"\r\r\n              size=\"icon\"\r\r\n              className=\"h-8 w-8\"\r\r\n              aria-label=\"Go to previous page\"\r\r\n            >\r\r\n              <ChevronLeft className=\"h-4 w-4\" />\r\r\n            </PaginationLink>\r\r\n          </PaginationItem>\r\r\n\r\r\n          <div className=\"flex items-center gap-2 mx-2\">\r\r\n            <PaginationItem>\r\r\n              <PaginationLink\r\r\n                href=\"#\"\r\r\n                className=\"h-3 w-3 p-0 rounded-full bg-muted hover:bg-muted-foreground/50 transition-colors\"\r\r\n                aria-label=\"Go to page 1\"\r\r\n              >\r\r\n                <span className=\"sr-only\">1</span>\r\r\n              </PaginationLink>\r\r\n            </PaginationItem>\r\r\n            <PaginationItem>\r\r\n              <PaginationLink\r\r\n                href=\"#\"\r\r\n                isActive\r\r\n                className=\"h-3 w-3 p-0 rounded-full bg-primary hover:bg-primary/80 transition-colors\"\r\r\n                aria-label=\"Current page, page 2\"\r\r\n              >\r\r\n                <span className=\"sr-only\">2</span>\r\r\n              </PaginationLink>\r\r\n            </PaginationItem>\r\r\n            <PaginationItem>\r\r\n              <PaginationLink\r\r\n                href=\"#\"\r\r\n                className=\"h-3 w-3 p-0 rounded-full bg-muted hover:bg-muted-foreground/50 transition-colors\"\r\r\n                aria-label=\"Go to page 3\"\r\r\n              >\r\r\n                <span className=\"sr-only\">3</span>\r\r\n              </PaginationLink>\r\r\n            </PaginationItem>\r\r\n            <PaginationItem>\r\r\n              <PaginationLink\r\r\n                href=\"#\"\r\r\n                className=\"h-3 w-3 p-0 rounded-full bg-muted hover:bg-muted-foreground/50 transition-colors\"\r\r\n                aria-label=\"Go to page 4\"\r\r\n              >\r\r\n                <span className=\"sr-only\">4</span>\r\r\n              </PaginationLink>\r\r\n            </PaginationItem>\r\r\n            <PaginationItem>\r\r\n              <PaginationLink\r\r\n                href=\"#\"\r\r\n                className=\"h-3 w-3 p-0 rounded-full bg-muted hover:bg-muted-foreground/50 transition-colors\"\r\r\n                aria-label=\"Go to page 5\"\r\r\n              >\r\r\n                <span className=\"sr-only\">5</span>\r\r\n              </PaginationLink>\r\r\n            </PaginationItem>\r\r\n          </div>\r\r\n\r\r\n          <PaginationItem>\r\r\n            <PaginationLink\r\r\n              href=\"#\"\r\r\n              size=\"icon\"\r\r\n              className=\"h-8 w-8\"\r\r\n              aria-label=\"Go to next page\"\r\r\n            >\r\r\n              <ChevronRight className=\"h-4 w-4\" />\r\r\n            </PaginationLink>\r\r\n          </PaginationItem>\r\r\n        </PaginationContent>\r\r\n      </Pagination>\r\r\n    </div>\r\r\n  );\r\r\n}\r\r\n",
      "type": "registry:block"
    }
