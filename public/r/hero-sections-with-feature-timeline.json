      "content": "import { Button } from \"@/registry/default/ui/button\";\r\r\nimport { Badge } from \"@/registry/default/ui/badge\";\r\r\nimport { Card } from \"@/registry/default/ui/card\";\r\r\n\r\r\nexport default function HeroSectionWithFeatureTimeline() {\r\r\n  const milestones = [\r\r\n    {\r\r\n      date: \"Q1 2024\",\r\r\n      title: \"AI-Powered Code Generation\",\r\r\n      description:\r\r\n        \"Intelligent code suggestions and automated refactoring powered by advanced machine learning models.\",\r\r\n      status: \"Released\",\r\r\n      icon: (\r\r\n        <svg\r\r\n          xmlns=\"http://www.w3.org/2000/svg\"\r\r\n          width=\"24\"\r\r\n          height=\"24\"\r\r\n          viewBox=\"0 0 24 24\"\r\r\n          fill=\"none\"\r\r\n          stroke=\"currentColor\"\r\r\n          strokeWidth=\"2\"\r\r\n          strokeLinecap=\"round\"\r\r\n          strokeLinejoin=\"round\"\r\r\n          className=\"text-primary\"\r\r\n        >\r\r\n          <path d=\"m18.5 5.5-8.5 8.5-4-4\" />\r\r\n          <path d=\"M18.5 5.5h-4v-4\" />\r\r\n          <circle cx=\"12\" cy=\"12\" r=\"10\" />\r\r\n        </svg>\r\r\n      ),\r\r\n    },\r\r\n    {\r\r\n      date: \"Q2 2024\",\r\r\n      title: \"Real-Time Collaboration\",\r\r\n      description:\r\r\n        \"Multi-user editing, commenting, and version control integration for seamless team workflows.\",\r\r\n      status: \"In Progress\",\r\r\n      icon: (\r\r\n        <svg\r\r\n          xmlns=\"http://www.w3.org/2000/svg\"\r\r\n          width=\"24\"\r\r\n          height=\"24\"\r\r\n          viewBox=\"0 0 24 24\"\r\r\n          fill=\"none\"\r\r\n          stroke=\"currentColor\"\r\r\n          strokeWidth=\"2\"\r\r\n          strokeLinecap=\"round\"\r\r\n          strokeLinejoin=\"round\"\r\r\n          className=\"text-primary\"\r\r\n        >\r\r\n          <path d=\"M12 22c5.523 0 10-4.477 10-10S17.523 2 12 2 2 6.477 2 12s4.477 10 10 10z\" />\r\r\n          <path d=\"M6 12h12\" />\r\r\n          <path d=\"M12 6v12\" />\r\r\n        </svg>\r\r\n      ),\r\r\n    },\r\r\n    {\r\r\n      date: \"Q3 2024\",\r\r\n      title: \"Advanced Testing Suite\",\r\r\n      description:\r\r\n        \"Automated testing framework with AI-driven test generation and coverage analysis.\",\r\r\n      status: \"Planned\",\r\r\n      icon: (\r\r\n        <svg\r\r\n          xmlns=\"http://www.w3.org/2000/svg\"\r\r\n          width=\"24\"\r\r\n          height=\"24\"\r\r\n          viewBox=\"0 0 24 24\"\r\r\n          fill=\"none\"\r\r\n          stroke=\"currentColor\"\r\r\n          strokeWidth=\"2\"\r\r\n          strokeLinecap=\"round\"\r\r\n          strokeLinejoin=\"round\"\r\r\n          className=\"text-primary\"\r\r\n        >\r\r\n          <path d=\"M12 22c5.523 0 10-4.477 10-10S17.523 2 12 2 2 6.477 2 12s4.477 10 10 10z\" />\r\r\n          <path d=\"M12 6v6l4 2\" />\r\r\n        </svg>\r\r\n      ),\r\r\n    },\r\r\n    {\r\r\n      date: \"Q4 2024\",\r\r\n      title: \"Enterprise Security Features\",\r\r\n      description:\r\r\n        \"Advanced security controls, audit logging, and compliance reporting for enterprise teams.\",\r\r\n      status: \"Planned\",\r\r\n      icon: (\r\r\n        <svg\r\r\n          xmlns=\"http://www.w3.org/2000/svg\"\r\r\n          width=\"24\"\r\r\n          height=\"24\"\r\r\n          viewBox=\"0 0 24 24\"\r\r\n          fill=\"none\"\r\r\n          stroke=\"currentColor\"\r\r\n          strokeWidth=\"2\"\r\r\n          strokeLinecap=\"round\"\r\r\n          strokeLinejoin=\"round\"\r\r\n          className=\"text-primary\"\r\r\n        >\r\r\n          <path d=\"M12 22s8-4 8-10V5l-8-3-8 3v7c0 6 8 10 8 10\" />\r\r\n        </svg>\r\r\n      ),\r\r\n    },\r\r\n  ];\r\r\n\r\r\n  return (\r\r\n    <>\r\r\n      <div className=\"\">\r\r\n        <div className=\"relative\">\r\r\n          {/* Background pattern */}\r\r\n          <div className=\"absolute inset-0 -z-10 h-full w-full \">\r\r\n            <div className=\"absolute h-full w-full bg-[linear-gradient(to_right,#80808012_1px,transparent_1px),linear-gradient(to_bottom,#80808012_1px,transparent_1px)] bg-[size:24px_24px]\"></div>\r\r\n            <div className=\"absolute bottom-0 left-0 right-0 top-0 bg-[radial-gradient(circle_800px_at_100%_200px,rgba(120,119,198,0.1),transparent)]\"></div>\r\r\n          </div>\r\r\n\r\r\n          <div className=\"container mx-auto px-4 md:px-6 2xl:max-w-[1400px] py-24 lg:py-32\">\r\r\n            <div className=\"text-center max-w-3xl mx-auto mb-16\">\r\r\n              <Badge variant=\"outline\" className=\"mb-4\">\r\r\n                Roadmap\r\r\n              </Badge>\r\r\n              <h1 className=\"scroll-m-20 text-4xl font-extrabold tracking-tight lg:text-5xl mb-4\">\r\r\n                Building the future of development\r\r\n              </h1>\r\r\n              <p className=\"text-xl text-muted-foreground mb-8\">\r\r\n                Our commitment to innovation drives us forward. See what\r\r\n                we&apos;re building and what&apos;s coming next.\r\r\n              </p>\r\r\n              <div className=\"flex flex-col sm:flex-row justify-center gap-3\">\r\r\n                <Button size=\"lg\">\r\r\n                  View Full Roadmap\r\r\n                  <svg\r\r\n                    xmlns=\"http://www.w3.org/2000/svg\"\r\r\n                    width=\"24\"\r\r\n                    height=\"24\"\r\r\n                    viewBox=\"0 0 24 24\"\r\r\n                    fill=\"none\"\r\r\n                    stroke=\"currentColor\"\r\r\n                    strokeWidth=\"2\"\r\r\n                    strokeLinecap=\"round\"\r\r\n                    strokeLinejoin=\"round\"\r\r\n                    className=\"ml-2 h-4 w-4\"\r\r\n                  >\r\r\n                    <path d=\"M5 12h14\" />\r\r\n                    <path d=\"m12 5 7 7-7 7\" />\r\r\n                  </svg>\r\r\n                </Button>\r\r\n                <Button size=\"lg\" variant=\"outline\">\r\r\n                  Request Feature\r\r\n                </Button>\r\r\n              </div>\r\r\n            </div>\r\r\n\r\r\n            {/* Timeline */}\r\r\n            <div className=\"max-w-4xl mx-auto\">\r\r\n              <div className=\"space-y-8\">\r\r\n                {milestones.map((milestone, index) => (\r\r\n                  <div key={milestone.date} className=\"relative\">\r\r\n                    {/* Timeline line */}\r\r\n                    {index !== milestones.length - 1 && (\r\r\n                      <div\r\r\n                        className={`absolute left-6 top-12 h-full w-0.5 ${\r\r\n                          milestone.status === \"Released\"\r\r\n                            ? \"bg-primary\"\r\r\n                            : milestone.status === \"In Progress\"\r\r\n                            ? \"bg-gradient-to-b from-primary to-muted\"\r\r\n                            : \"bg-muted\"\r\r\n                        }`}\r\r\n                      ></div>\r\r\n                    )}\r\r\n\r\r\n                    <Card className=\"relative flex gap-6 p-6 hover:shadow-lg transition-all duration-300\">\r\r\n                      {/* Icon */}\r\r\n                      <div\r\r\n                        className={`w-12 h-12 rounded-full flex items-center justify-center ${\r\r\n                          milestone.status === \"Released\"\r\r\n                            ? \"bg-primary/10\"\r\r\n                            : milestone.status === \"In Progress\"\r\r\n                            ? \"bg-primary/5\"\r\r\n                            : \"bg-muted\"\r\r\n                        }`}\r\r\n                      >\r\r\n                        {milestone.icon}\r\r\n                      </div>\r\r\n\r\r\n                      <div className=\"flex-1\">\r\r\n                        <div className=\"flex items-center gap-3 mb-2\">\r\r\n                          <Badge\r\r\n                            variant={\r\r\n                              milestone.status === \"Released\"\r\r\n                                ? \"default\"\r\r\n                                : milestone.status === \"In Progress\"\r\r\n                                ? \"secondary\"\r\r\n                                : \"outline\"\r\r\n                            }\r\r\n                          >\r\r\n                            {milestone.status}\r\r\n                          </Badge>\r\r\n                          <div className=\"text-sm text-muted-foreground\">\r\r\n                            {milestone.date}\r\r\n                          </div>\r\r\n                        </div>\r\r\n                        <h3 className=\"font-semibold mb-2\">\r\r\n                          {milestone.title}\r\r\n                        </h3>\r\r\n                        <p className=\"text-sm text-muted-foreground\">\r\r\n                          {milestone.description}\r\r\n                        </p>\r\r\n                      </div>\r\r\n                    </Card>\r\r\n                  </div>\r\r\n                ))}\r\r\n              </div>\r\r\n            </div>\r\r\n\r\r\n            {/* Feature Request CTA */}\r\r\n            <Card className=\"mt-16 p-8 text-center max-w-2xl mx-auto bg-muted/50\">\r\r\n              <h3 className=\"font-semibold mb-2\">Have a feature in mind?</h3>\r\r\n              <p className=\"text-sm text-muted-foreground mb-4\">\r\r\n                We&apos;re always looking to improve. Share your ideas and help\r\r\n                shape our roadmap.\r\r\n              </p>\r\r\n              <Button variant=\"outline\" size=\"lg\">\r\r\n                Submit Feature Request\r\r\n              </Button>\r\r\n            </Card>\r\r\n          </div>\r\r\n        </div>\r\r\n      </div>\r\r\n    </>\r\r\n  );\r\r\n}\r\r\n",
