{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "form-layouts-complex-form",
  "type": "registry:block",
  "registryDependencies": [
    "https://ui.woilasoft.com/r/button.json",
    "https://ui.woilasoft.com/r/card.json",
    "https://ui.woilasoft.com/r/checkbox.json",
    "https://ui.woilasoft.com/r/input.json",
    "https://ui.woilasoft.com/r/label.json",
    "https://ui.woilasoft.com/r/radio-group.json",
    "https://ui.woilasoft.com/r/select.json",
    "https://ui.woilasoft.com/r/separator.json"
  ],
  "files": [
    {
      "path": "registry/default/blocks/form-layouts-complex-form.tsx",
      "content": "\"use client\";\r\r\n\r\r\nimport { Button } from \"@/registry/default/ui/button\";\r\r\nimport { Card, CardContent, CardHeader, CardTitle } from \"@/registry/default/ui/card\";\r\r\nimport { Checkbox } from \"@/registry/default/ui/checkbox\";\r\r\nimport {\r\r\n  Select,\r\r\n  SelectContent,\r\r\n  SelectItem,\r\r\n  SelectTrigger,\r\r\n  SelectValue,\r\r\n} from \"@/registry/default/ui/select\";\r\r\nimport { Input } from \"@/registry/default/ui/input\";\r\r\nimport { Label } from \"@/registry/default/ui/label\";\r\r\nimport { RadioGroup, RadioGroupItem } from \"@/registry/default/ui/radio-group\";\r\r\nimport { Separator } from \"@/registry/default/ui/separator\";\r\r\nimport { UploadCloud } from \"lucide-react\";\r\r\nimport { useState } from \"react\";\r\r\nimport Image from \"next/image\";\r\r\n\r\r\nexport default function ComplexForm() {\r\r\n  const [imagePreview, setImagePreview] = useState<string | null>(null);\r\r\n\r\r\n  const handleImageUpload = (event: React.ChangeEvent<HTMLInputElement>) => {\r\r\n    const file = event.target.files?.[0];\r\r\n    if (file) {\r\r\n      const reader = new FileReader();\r\r\n      reader.onloadend = () => {\r\r\n        setImagePreview(reader.result as string);\r\r\n      };\r\r\n      reader.readAsDataURL(file);\r\r\n    }\r\r\n  };\r\r\n\r\r\n  return (\r\r\n    <div className=\"container mx-auto px-4 md:px-6 2xl:max-w-[1400px] py-10\">\r\r\n      <Card>\r\r\n        <CardHeader>\r\r\n          <CardTitle>Create Product Listing</CardTitle>\r\r\n        </CardHeader>\r\r\n        <CardContent>\r\r\n          <form className=\"space-y-8\">\r\r\n            {/* Basic Information */}\r\r\n            <div className=\"space-y-6\">\r\r\n              <div>\r\r\n                <h3 className=\"text-lg font-medium\">Basic Information</h3>\r\r\n                <p className=\"text-sm text-muted-foreground\">\r\r\n                  Add your product details below.\r\r\n                </p>\r\r\n              </div>\r\r\n\r\r\n              <div className=\"space-y-4\">\r\r\n                <div className=\"space-y-2\">\r\r\n                  <Label htmlFor=\"name\">Product Name</Label>\r\r\n                  <Input id=\"name\" placeholder=\"Enter product name\" />\r\r\n                </div>\r\r\n\r\r\n                <div className=\"space-y-2\">\r\r\n                  <Label>Product Category</Label>\r\r\n                  <Select>\r\r\n                    <SelectTrigger>\r\r\n                      <SelectValue placeholder=\"Select category\" />\r\r\n                    </SelectTrigger>\r\r\n                    <SelectContent>\r\r\n                      <SelectItem value=\"electronics\">Electronics</SelectItem>\r\r\n                      <SelectItem value=\"clothing\">Clothing</SelectItem>\r\r\n                      <SelectItem value=\"books\">Books</SelectItem>\r\r\n                      <SelectItem value=\"home\">Home & Garden</SelectItem>\r\r\n                    </SelectContent>\r\r\n                  </Select>\r\r\n                </div>\r\r\n\r\r\n                <div className=\"space-y-2\">\r\r\n                  <Label>Availability</Label>\r\r\n                  <RadioGroup\r\r\n                    defaultValue=\"instock\"\r\r\n                    className=\"flex flex-col sm:flex-row gap-4\"\r\r\n                  >\r\r\n                    <div className=\"flex items-center space-x-2\">\r\r\n                      <RadioGroupItem value=\"instock\" id=\"instock\" />\r\r\n                      <Label htmlFor=\"instock\">In Stock</Label>\r\r\n                    </div>\r\r\n                    <div className=\"flex items-center space-x-2\">\r\r\n                      <RadioGroupItem value=\"preorder\" id=\"preorder\" />\r\r\n                      <Label htmlFor=\"preorder\">Pre-order</Label>\r\r\n                    </div>\r\r\n                    <div className=\"flex items-center space-x-2\">\r\r\n                      <RadioGroupItem value=\"backorder\" id=\"backorder\" />\r\r\n                      <Label htmlFor=\"backorder\">Backorder</Label>\r\r\n                    </div>\r\r\n                  </RadioGroup>\r\r\n                </div>\r\r\n              </div>\r\r\n            </div>\r\r\n\r\r\n            <Separator />\r\r\n\r\r\n            {/* Media Upload */}\r\r\n            <div className=\"space-y-6\">\r\r\n              <div>\r\r\n                <h3 className=\"text-lg font-medium\">Product Media</h3>\r\r\n                <p className=\"text-sm text-muted-foreground\">\r\r\n                  Add product images and media.\r\r\n                </p>\r\r\n              </div>\r\r\n\r\r\n              <div className=\"space-y-4\">\r\r\n                <div className=\"space-y-2\">\r\r\n                  <Label>Product Images</Label>\r\r\n                  <div className=\"flex items-center justify-center w-full\">\r\r\n                    <label\r\r\n                      htmlFor=\"image-upload\"\r\r\n                      className=\"flex flex-col items-center justify-center w-full h-64 border-2 border-dashed rounded-lg cursor-pointer hover:bg-muted/50\"\r\r\n                    >\r\r\n                      {imagePreview ? (\r\r\n                        <Image\r\r\n                          width={1280}\r\r\n                          height={720}\r\r\n                          src={imagePreview}\r\r\n                          alt=\"Preview\"\r\r\n                          className=\"h-full object-contain\"\r\r\n                        />\r\r\n                      ) : (\r\r\n                        <div className=\"flex flex-col items-center justify-center pt-5 pb-6\">\r\r\n                          <UploadCloud className=\"h-12 w-12 text-muted-foreground mb-2\" />\r\r\n                          <p className=\"mb-2 text-sm text-muted-foreground\">\r\r\n                            <span className=\"font-semibold\">\r\r\n                              Click to upload\r\r\n                            </span>{\" \"}\r\r\n                            or drag and drop\r\r\n                          </p>\r\r\n                          <p className=\"text-xs text-muted-foreground\">\r\r\n                            PNG, JPG or GIF (MAX. 800x400px)\r\r\n                          </p>\r\r\n                        </div>\r\r\n                      )}\r\r\n                      <Input\r\r\n                        id=\"image-upload\"\r\r\n                        type=\"file\"\r\r\n                        className=\"hidden\"\r\r\n                        accept=\"image/*\"\r\r\n                        onChange={handleImageUpload}\r\r\n                      />\r\r\n                    </label>\r\r\n                  </div>\r\r\n                </div>\r\r\n              </div>\r\r\n            </div>\r\r\n\r\r\n            <Separator />\r\r\n\r\r\n            {/* Additional Options */}\r\r\n            <div className=\"space-y-6\">\r\r\n              <div>\r\r\n                <h3 className=\"text-lg font-medium\">Additional Options</h3>\r\r\n                <p className=\"text-sm text-muted-foreground\">\r\r\n                  Configure additional product options.\r\r\n                </p>\r\r\n              </div>\r\r\n\r\r\n              <div className=\"space-y-4\">\r\r\n                <div className=\"space-y-2\">\r\r\n                  <Label>Features</Label>\r\r\n                  <div className=\"grid sm:grid-cols-2 gap-4\">\r\r\n                    <div className=\"flex items-center space-x-2\">\r\r\n                      <Checkbox id=\"featured\" />\r\r\n                      <Label htmlFor=\"featured\">Featured Product</Label>\r\r\n                    </div>\r\r\n                    <div className=\"flex items-center space-x-2\">\r\r\n                      <Checkbox id=\"sale\" />\r\r\n                      <Label htmlFor=\"sale\">On Sale</Label>\r\r\n                    </div>\r\r\n                    <div className=\"flex items-center space-x-2\">\r\r\n                      <Checkbox id=\"new\" />\r\r\n                      <Label htmlFor=\"new\">New Arrival</Label>\r\r\n                    </div>\r\r\n                    <div className=\"flex items-center space-x-2\">\r\r\n                      <Checkbox id=\"exclusive\" />\r\r\n                      <Label htmlFor=\"exclusive\">Exclusive</Label>\r\r\n                    </div>\r\r\n                  </div>\r\r\n                </div>\r\r\n              </div>\r\r\n            </div>\r\r\n\r\r\n            <div className=\"flex justify-end gap-4\">\r\r\n              <Button variant=\"outline\">Cancel</Button>\r\r\n              <Button>Create Product</Button>\r\r\n            </div>\r\r\n          </form>\r\r\n        </CardContent>\r\r\n      </Card>\r\r\n    </div>\r\r\n  );\r\r\n}\r\r\n",
      "type": "registry:block"
    }
  ],
  "meta": {
    "tags": [
      "form-layouts"
    ]
  }
}