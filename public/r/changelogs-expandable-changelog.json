      "path": "registry/default/blocks/changelogs-expandable-changelog.tsx",
      "content": "import { Badge } from \"@/registry/default/ui/badge\";\r\r\nimport {\r\r\n  Accordion,\r\r\n  AccordionContent,\r\r\n  AccordionItem,\r\r\n  AccordionTrigger,\r\r\n} from \"@/registry/default/ui/accordion\";\r\r\n\r\r\nexport default function ExpandableChangelog() {\r\r\n  return (\r\r\n    <div className=\"container mx-auto max-w-3xl px-4 py-6 sm:py-8\">\r\r\n      <div className=\"mb-6 sm:mb-8\">\r\r\n        <h2 className=\"text-2xl font-bold tracking-tight sm:text-3xl\">\r\r\n          Changelog\r\r\n        </h2>\r\r\n        <p className=\"text-muted-foreground mt-2\">\r\r\n          Expand each version to see the changes\r\r\n        </p>\r\r\n      </div>\r\r\n\r\r\n      <Accordion\r\r\n        type=\"single\"\r\r\n        collapsible\r\r\n        className=\"w-full\"\r\r\n        defaultValue=\"v2.0.0\"\r\r\n      >\r\r\n        <AccordionItem\r\r\n          value=\"v2.0.0\"\r\r\n          className=\"mb-3 overflow-hidden rounded-lg border sm:mb-4\"\r\r\n        >\r\r\n          <AccordionTrigger className=\"hover:bg-muted/40 [&[data-state=open]]:bg-muted/40 px-3 py-3 sm:px-6 sm:py-4\">\r\r\n            <div className=\"flex w-full flex-wrap items-center gap-2 sm:gap-3\">\r\r\n              <span className=\"text-base font-medium sm:text-lg\">\r\r\n                Version 2.0.0\r\r\n              </span>\r\r\n              <Badge className=\"text-xs\">Latest</Badge>\r\r\n              <span className=\"text-muted-foreground text-xs\">\r\r\n                April 15, 2023\r\r\n              </span>\r\r\n            </div>\r\r\n          </AccordionTrigger>\r\r\n          <AccordionContent className=\"px-3 pt-2 pb-4 sm:px-6\">\r\r\n            <div className=\"grid grid-cols-1 gap-4 sm:gap-6 md:grid-cols-2\">\r\r\n              <div>\r\r\n                <h4 className=\"text-muted-foreground mb-2 text-xs font-semibold tracking-wider uppercase sm:mb-3 sm:text-sm\">\r\r\n                  Features & Improvements\r\r\n                </h4>\r\r\n                <ul className=\"space-y-2 sm:space-y-3\">\r\r\n                  <li className=\"flex gap-2\">\r\r\n                    <svg\r\r\n                      xmlns=\"http://www.w3.org/2000/svg\"\r\r\n                      width=\"18\"\r\r\n                      height=\"18\"\r\r\n                      viewBox=\"0 0 24 24\"\r\r\n                      fill=\"none\"\r\r\n                      stroke=\"currentColor\"\r\r\n                      strokeWidth=\"2\"\r\r\n                      strokeLinecap=\"round\"\r\r\n                      strokeLinejoin=\"round\"\r\r\n                      className=\"mt-1 flex-shrink-0 text-green-500\"\r\r\n                    >\r\r\n                      <polyline points=\"20 6 9 17 4 12\"></polyline>\r\r\n                    </svg>\r\r\n                    <div>\r\r\n                      <p className=\"text-sm font-medium sm:text-base\">\r\r\n                        Redesigned dashboard\r\r\n                      </p>\r\r\n                      <p className=\"text-muted-foreground text-xs sm:text-sm\">\r\r\n                        More intuitive UI with improved accessibility\r\r\n                      </p>\r\r\n                    </div>\r\r\n                  </li>\r\r\n                  <li className=\"flex gap-2\">\r\r\n                    <svg\r\r\n                      xmlns=\"http://www.w3.org/2000/svg\"\r\r\n                      width=\"18\"\r\r\n                      height=\"18\"\r\r\n                      viewBox=\"0 0 24 24\"\r\r\n                      fill=\"none\"\r\r\n                      stroke=\"currentColor\"\r\r\n                      strokeWidth=\"2\"\r\r\n                      strokeLinecap=\"round\"\r\r\n                      strokeLinejoin=\"round\"\r\r\n                      className=\"mt-1 flex-shrink-0 text-green-500\"\r\r\n                    >\r\r\n                      <polyline points=\"20 6 9 17 4 12\"></polyline>\r\r\n                    </svg>\r\r\n                    <div>\r\r\n                      <p className=\"text-sm font-medium sm:text-base\">\r\r\n                        Dark mode support\r\r\n                      </p>\r\r\n                      <p className=\"text-muted-foreground text-xs sm:text-sm\">\r\r\n                        Full dark mode across all pages and components\r\r\n                      </p>\r\r\n                    </div>\r\r\n                  </li>\r\r\n                  <li className=\"flex gap-2\">\r\r\n                    <svg\r\r\n                      xmlns=\"http://www.w3.org/2000/svg\"\r\r\n                      width=\"18\"\r\r\n                      height=\"18\"\r\r\n                      viewBox=\"0 0 24 24\"\r\r\n                      fill=\"none\"\r\r\n                      stroke=\"currentColor\"\r\r\n                      strokeWidth=\"2\"\r\r\n                      strokeLinecap=\"round\"\r\r\n                      strokeLinejoin=\"round\"\r\r\n                      className=\"mt-1 flex-shrink-0 text-blue-500\"\r\r\n                    >\r\r\n                      <polyline points=\"20 6 9 17 4 12\"></polyline>\r\r\n                    </svg>\r\r\n                    <div>\r\r\n                      <p className=\"text-sm font-medium sm:text-base\">\r\r\n                        Performance improvements\r\r\n                      </p>\r\r\n                      <p className=\"text-muted-foreground text-xs sm:text-sm\">\r\r\n                        40% faster load times and reduced bundle size\r\r\n                      </p>\r\r\n                    </div>\r\r\n                  </li>\r\r\n                </ul>\r\r\n              </div>\r\r\n              <div>\r\r\n                <h4 className=\"text-muted-foreground mb-2 text-xs font-semibold tracking-wider uppercase sm:mb-3 sm:text-sm\">\r\r\n                  Bug Fixes & Changes\r\r\n                </h4>\r\r\n                <ul className=\"space-y-2 sm:space-y-3\">\r\r\n                  <li className=\"flex gap-2\">\r\r\n                    <svg\r\r\n                      xmlns=\"http://www.w3.org/2000/svg\"\r\r\n                      width=\"18\"\r\r\n                      height=\"18\"\r\r\n                      viewBox=\"0 0 24 24\"\r\r\n                      fill=\"none\"\r\r\n                      stroke=\"currentColor\"\r\r\n                      strokeWidth=\"2\"\r\r\n                      strokeLinecap=\"round\"\r\r\n                      strokeLinejoin=\"round\"\r\r\n                      className=\"mt-1 flex-shrink-0 text-red-500\"\r\r\n                    >\r\r\n                      <polyline points=\"20 6 9 17 4 12\"></polyline>\r\r\n                    </svg>\r\r\n                    <div>\r\r\n                      <p className=\"text-sm font-medium sm:text-base\">\r\r\n                        Fixed accessibility issues\r\r\n                      </p>\r\r\n                      <p className=\"text-muted-foreground text-xs sm:text-sm\">\r\r\n                        Improved keyboard navigation and screen reader support\r\r\n                      </p>\r\r\n                    </div>\r\r\n                  </li>\r\r\n                  <li className=\"flex gap-2\">\r\r\n                    <svg\r\r\n                      xmlns=\"http://www.w3.org/2000/svg\"\r\r\n                      width=\"18\"\r\r\n                      height=\"18\"\r\r\n                      viewBox=\"0 0 24 24\"\r\r\n                      fill=\"none\"\r\r\n                      stroke=\"currentColor\"\r\r\n                      strokeWidth=\"2\"\r\r\n                      strokeLinecap=\"round\"\r\r\n                      strokeLinejoin=\"round\"\r\r\n                      className=\"mt-1 flex-shrink-0 text-yellow-500\"\r\r\n                    >\r\r\n                      <polyline points=\"20 6 9 17 4 12\"></polyline>\r\r\n                    </svg>\r\r\n                    <div>\r\r\n                      <p className=\"text-sm font-medium sm:text-base\">\r\r\n                        API changes\r\r\n                      </p>\r\r\n                      <p className=\"text-muted-foreground text-xs sm:text-sm\">\r\r\n                        Restructured endpoints with backward compatibility\r\r\n                      </p>\r\r\n                    </div>\r\r\n                  </li>\r\r\n                </ul>\r\r\n              </div>\r\r\n            </div>\r\r\n          </AccordionContent>\r\r\n        </AccordionItem>\r\r\n\r\r\n        <AccordionItem\r\r\n          value=\"v1.2.0\"\r\r\n          className=\"mb-3 overflow-hidden rounded-lg border sm:mb-4\"\r\r\n        >\r\r\n          <AccordionTrigger className=\"hover:bg-muted/40 [&[data-state=open]]:bg-muted/40 px-3 py-3 sm:px-6 sm:py-4\">\r\r\n            <div className=\"flex w-full flex-wrap items-center gap-2 sm:gap-3\">\r\r\n              <span className=\"text-base font-medium sm:text-lg\">\r\r\n                Version 1.2.0\r\r\n              </span>\r\r\n              <span className=\"text-muted-foreground text-xs\">\r\r\n                March 2, 2023\r\r\n              </span>\r\r\n            </div>\r\r\n          </AccordionTrigger>\r\r\n          <AccordionContent className=\"px-3 pt-2 pb-4 sm:px-6\">\r\r\n            <div className=\"space-y-3 sm:space-y-4\">\r\r\n              <div>\r\r\n                <h4 className=\"text-muted-foreground mb-2 text-xs font-semibold tracking-wider uppercase sm:mb-3 sm:text-sm\">\r\r\n                  Features & Improvements\r\r\n                </h4>\r\r\n                <ul className=\"space-y-2\">\r\r\n                  <li className=\"flex gap-2\">\r\r\n                    <svg\r\r\n                      xmlns=\"http://www.w3.org/2000/svg\"\r\r\n                      width=\"18\"\r\r\n                      height=\"18\"\r\r\n                      viewBox=\"0 0 24 24\"\r\r\n                      fill=\"none\"\r\r\n                      stroke=\"currentColor\"\r\r\n                      strokeWidth=\"2\"\r\r\n                      strokeLinecap=\"round\"\r\r\n                      strokeLinejoin=\"round\"\r\r\n                      className=\"mt-0.5 flex-shrink-0 text-green-500\"\r\r\n                    >\r\r\n                      <polyline points=\"20 6 9 17 4 12\"></polyline>\r\r\n                    </svg>\r\r\n                    <p className=\"text-sm sm:text-base\">\r\r\n                      Added new analytics dashboard with advanced filters\r\r\n                    </p>\r\r\n                  </li>\r\r\n                  <li className=\"flex gap-2\">\r\r\n                    <svg\r\r\n                      xmlns=\"http://www.w3.org/2000/svg\"\r\r\n                      width=\"18\"\r\r\n                      height=\"18\"\r\r\n                      viewBox=\"0 0 24 24\"\r\r\n                      fill=\"none\"\r\r\n                      stroke=\"currentColor\"\r\r\n                      strokeWidth=\"2\"\r\r\n                      strokeLinecap=\"round\"\r\r\n                      strokeLinejoin=\"round\"\r\r\n                      className=\"mt-0.5 flex-shrink-0 text-blue-500\"\r\r\n                    >\r\r\n                      <polyline points=\"20 6 9 17 4 12\"></polyline>\r\r\n                    </svg>\r\r\n                    <p className=\"text-sm sm:text-base\">\r\r\n                      Improved mobile responsiveness across all views\r\r\n                    </p>\r\r\n                  </li>\r\r\n                </ul>\r\r\n              </div>\r\r\n              <div>\r\r\n                <h4 className=\"text-muted-foreground mb-2 text-xs font-semibold tracking-wider uppercase sm:mb-3 sm:text-sm\">\r\r\n                  Bug Fixes\r\r\n                </h4>\r\r\n                <ul className=\"space-y-2\">\r\r\n                  <li className=\"flex gap-2\">\r\r\n                    <svg\r\r\n                      xmlns=\"http://www.w3.org/2000/svg\"\r\r\n                      width=\"18\"\r\r\n                      height=\"18\"\r\r\n                      viewBox=\"0 0 24 24\"\r\r\n                      fill=\"none\"\r\r\n                      stroke=\"currentColor\"\r\r\n                      strokeWidth=\"2\"\r\r\n                      strokeLinecap=\"round\"\r\r\n                      strokeLinejoin=\"round\"\r\r\n                      className=\"mt-0.5 flex-shrink-0 text-red-500\"\r\r\n                    >\r\r\n                      <polyline points=\"20 6 9 17 4 12\"></polyline>\r\r\n                    </svg>\r\r\n                    <p className=\"text-sm sm:text-base\">\r\r\n                      Fixed bug with user authentication\r\r\n                    </p>\r\r\n                  </li>\r\r\n                </ul>\r\r\n              </div>\r\r\n            </div>\r\r\n          </AccordionContent>\r\r\n        </AccordionItem>\r\r\n\r\r\n        <AccordionItem\r\r\n          value=\"v1.1.0\"\r\r\n          className=\"mb-3 overflow-hidden rounded-lg border sm:mb-4\"\r\r\n        >\r\r\n          <AccordionTrigger className=\"hover:bg-muted/40 [&[data-state=open]]:bg-muted/40 px-3 py-3 sm:px-6 sm:py-4\">\r\r\n            <div className=\"flex w-full flex-wrap items-center gap-2 sm:gap-3\">\r\r\n              <span className=\"text-base font-medium sm:text-lg\">\r\r\n                Version 1.1.0\r\r\n              </span>\r\r\n              <span className=\"text-muted-foreground text-xs\">\r\r\n                January 15, 2023\r\r\n              </span>\r\r\n            </div>\r\r\n          </AccordionTrigger>\r\r\n          <AccordionContent className=\"px-3 pt-2 pb-4 sm:px-6\">\r\r\n            <div className=\"space-y-3 sm:space-y-4\">\r\r\n              <div>\r\r\n                <h4 className=\"text-muted-foreground mb-2 text-xs font-semibold tracking-wider uppercase sm:mb-3 sm:text-sm\">\r\r\n                  What&apos;s New\r\r\n                </h4>\r\r\n                <ul className=\"space-y-2\">\r\r\n                  <li className=\"flex gap-2\">\r\r\n                    <svg\r\r\n                      xmlns=\"http://www.w3.org/2000/svg\"\r\r\n                      width=\"18\"\r\r\n                      height=\"18\"\r\r\n                      viewBox=\"0 0 24 24\"\r\r\n                      fill=\"none\"\r\r\n                      stroke=\"currentColor\"\r\r\n                      strokeWidth=\"2\"\r\r\n                      strokeLinecap=\"round\"\r\r\n                      strokeLinejoin=\"round\"\r\r\n                      className=\"mt-0.5 flex-shrink-0 text-green-500\"\r\r\n                    >\r\r\n                      <polyline points=\"20 6 9 17 4 12\"></polyline>\r\r\n                    </svg>\r\r\n                    <p className=\"text-sm sm:text-base\">\r\r\n                      Added support for custom themes\r\r\n                    </p>\r\r\n                  </li>\r\r\n                  <li className=\"flex gap-2\">\r\r\n                    <svg\r\r\n                      xmlns=\"http://www.w3.org/2000/svg\"\r\r\n                      width=\"18\"\r\r\n                      height=\"18\"\r\r\n                      viewBox=\"0 0 24 24\"\r\r\n                      fill=\"none\"\r\r\n                      stroke=\"currentColor\"\r\r\n                      strokeWidth=\"2\"\r\r\n                      strokeLinecap=\"round\"\r\r\n                      strokeLinejoin=\"round\"\r\r\n                      className=\"mt-0.5 flex-shrink-0 text-green-500\"\r\r\n                    >\r\r\n                      <polyline points=\"20 6 9 17 4 12\"></polyline>\r\r\n                    </svg>\r\r\n                    <p className=\"text-sm sm:text-base\">\r\r\n                      Improved form validation with helpful error messages\r\r\n                    </p>\r\r\n                  </li>\r\r\n                  <li className=\"flex gap-2\">\r\r\n                    <svg\r\r\n                      xmlns=\"http://www.w3.org/2000/svg\"\r\r\n                      width=\"18\"\r\r\n                      height=\"18\"\r\r\n                      viewBox=\"0 0 24 24\"\r\r\n                      fill=\"none\"\r\r\n                      stroke=\"currentColor\"\r\r\n                      strokeWidth=\"2\"\r\r\n                      strokeLinecap=\"round\"\r\r\n                      strokeLinejoin=\"round\"\r\r\n                      className=\"mt-0.5 flex-shrink-0 text-blue-500\"\r\r\n                    >\r\r\n                      <polyline points=\"20 6 9 17 4 12\"></polyline>\r\r\n                    </svg>\r\r\n                    <p className=\"text-sm sm:text-base\">\r\r\n                      Updated documentation with more examples\r\r\n                    </p>\r\r\n                  </li>\r\r\n                </ul>\r\r\n              </div>\r\r\n            </div>\r\r\n          </AccordionContent>\r\r\n        </AccordionItem>\r\r\n\r\r\n        <AccordionItem\r\r\n          value=\"v1.0.0\"\r\r\n          className=\"overflow-hidden rounded-lg !border\"\r\r\n        >\r\r\n          <AccordionTrigger className=\"hover:bg-muted/40 [&[data-state=open]]:bg-muted/40 px-3 py-3 sm:px-6 sm:py-4\">\r\r\n            <div className=\"flex w-full flex-wrap items-center gap-2 sm:gap-3\">\r\r\n              <span className=\"text-base font-medium sm:text-lg\">\r\r\n                Version 1.0.0\r\r\n              </span>\r\r\n              <span className=\"text-muted-foreground text-xs\">\r\r\n                December 1, 2022\r\r\n              </span>\r\r\n            </div>\r\r\n          </AccordionTrigger>\r\r\n          <AccordionContent className=\"px-3 pt-2 pb-4 sm:px-6\">\r\r\n            <p className=\"text-muted-foreground text-sm sm:text-base\">\r\r\n              Initial release of the product.\r\r\n            </p>\r\r\n          </AccordionContent>\r\r\n        </AccordionItem>\r\r\n      </Accordion>\r\r\n    </div>\r\r\n  );\r\r\n}\r\r\n",
