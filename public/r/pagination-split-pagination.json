      "content": "import {\r\r\n  Pagination,\r\r\n  PaginationContent,\r\r\n  PaginationEllipsis,\r\r\n  PaginationItem,\r\r\n  PaginationLink,\r\r\n  PaginationNext,\r\r\n  PaginationPrevious,\r\r\n} from \"@/registry/default/ui/pagination\";\r\r\n\r\r\nexport default function SplitPagination() {\r\r\n  return (\r\r\n    <div className=\"p-6\">\r\r\n      <div className=\"flex flex-col sm:flex-row justify-between items-center gap-4\">\r\r\n        {/* Previous button */}\r\r\n        <Pagination>\r\r\n          <PaginationContent>\r\r\n            <PaginationItem>\r\r\n              <PaginationPrevious href=\"#\" />\r\r\n            </PaginationItem>\r\r\n          </PaginationContent>\r\r\n        </Pagination>\r\r\n\r\r\n        {/* Page numbers */}\r\r\n        <Pagination>\r\r\n          <PaginationContent>\r\r\n            <PaginationItem>\r\r\n              <PaginationLink href=\"#\">1</PaginationLink>\r\r\n            </PaginationItem>\r\r\n            <PaginationItem>\r\r\n              <PaginationLink href=\"#\" isActive>\r\r\n                2\r\r\n              </PaginationLink>\r\r\n            </PaginationItem>\r\r\n            <PaginationItem>\r\r\n              <PaginationLink href=\"#\">3</PaginationLink>\r\r\n            </PaginationItem>\r\r\n            <PaginationItem>\r\r\n              <PaginationEllipsis />\r\r\n            </PaginationItem>\r\r\n            <PaginationItem>\r\r\n              <PaginationLink href=\"#\">8</PaginationLink>\r\r\n            </PaginationItem>\r\r\n          </PaginationContent>\r\r\n        </Pagination>\r\r\n\r\r\n        {/* Next button */}\r\r\n        <Pagination>\r\r\n          <PaginationContent>\r\r\n            <PaginationItem>\r\r\n              <PaginationNext href=\"#\" />\r\r\n            </PaginationItem>\r\r\n          </PaginationContent>\r\r\n        </Pagination>\r\r\n      </div>\r\r\n    </div>\r\r\n  );\r\r\n}\r\r\n",
      "type": "registry:block"
    }
