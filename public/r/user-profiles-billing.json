      "content": "import { Button } from \"@/registry/default/ui/button\";\r\r\nimport { Card, CardContent } from \"@/registry/default/ui/card\";\r\r\nimport { Badge } from \"@/registry/default/ui/badge\";\r\r\nimport { Progress } from \"@/registry/default/ui/progress\";\r\r\nimport {\r\r\n  CreditCard,\r\r\n  Download,\r\r\n  FileText,\r\r\n  Package,\r\r\n  RefreshCw,\r\r\n  Settings,\r\r\n  Zap,\r\r\n} from \"lucide-react\";\r\r\n\r\r\nconst invoices = [\r\r\n  {\r\r\n    id: \"INV-001\",\r\r\n    date: \"Mar 1, 2024\",\r\r\n    amount: \"$29.00\",\r\r\n    status: \"Paid\",\r\r\n  },\r\r\n  {\r\r\n    id: \"INV-002\",\r\r\n    date: \"Feb 1, 2024\",\r\r\n    amount: \"$29.00\",\r\r\n    status: \"Paid\",\r\r\n  },\r\r\n  {\r\r\n    id: \"INV-003\",\r\r\n    date: \"Jan 1, 2024\",\r\r\n    amount: \"$29.00\",\r\r\n    status: \"Paid\",\r\r\n  },\r\r\n];\r\r\n\r\r\nexport default function UserBilling() {\r\r\n  return (\r\r\n    <div className=\"container mx-auto px-4 py-6 md:px-6 2xl:max-w-[1400px]\">\r\r\n      <div className=\"mx-auto max-w-4xl\">\r\r\n        {/* Header */}\r\r\n        <div className=\"mb-8 flex flex-col items-start justify-between gap-4 sm:flex-row\">\r\r\n          <div>\r\r\n            <h1 className=\"text-2xl font-semibold\">Billing & Subscription</h1>\r\r\n            <p className=\"text-muted-foreground text-sm\">\r\r\n              Manage your subscription and billing details\r\r\n            </p>\r\r\n          </div>\r\r\n          <Button variant=\"outline\">\r\r\n            <Settings className=\"mr-2 size-4\" />\r\r\n            Billing Settings\r\r\n          </Button>\r\r\n        </div>\r\r\n\r\r\n        {/* Current Plan */}\r\r\n        <Card className=\"mb-8 p-0\">\r\r\n          <CardContent className=\"p-6\">\r\r\n            <div className=\"flex flex-col items-start justify-between gap-6 sm:flex-row\">\r\r\n              <div>\r\r\n                <div className=\"flex items-center gap-2\">\r\r\n                  <Package className=\"text-primary size-5\" />\r\r\n                  <h2 className=\"text-lg font-semibold\">Pro Plan</h2>\r\r\n                  <Badge>Current Plan</Badge>\r\r\n                </div>\r\r\n                <p className=\"text-muted-foreground mt-1 text-sm\">\r\r\n                  $29/month â€¢ Renews on April 1, 2024\r\r\n                </p>\r\r\n              </div>\r\r\n              <div className=\"flex flex-wrap gap-2\">\r\r\n                <Button variant=\"outline\">Change Plan</Button>\r\r\n                <Button variant=\"destructive\">Cancel Plan</Button>\r\r\n              </div>\r\r\n            </div>\r\r\n\r\r\n            <div className=\"mt-6 space-y-4\">\r\r\n              <div>\r\r\n                <div className=\"mb-2 flex items-center justify-between\">\r\r\n                  <div className=\"flex items-center gap-2\">\r\r\n                    <Zap className=\"text-primary size-4\" />\r\r\n                    <span className=\"text-sm font-medium\">API Requests</span>\r\r\n                  </div>\r\r\n                  <span className=\"text-sm\">8,543 / 10,000</span>\r\r\n                </div>\r\r\n                <Progress value={85.43} className=\"h-2\" />\r\r\n              </div>\r\r\n\r\r\n              <div>\r\r\n                <div className=\"mb-2 flex items-center justify-between\">\r\r\n                  <div className=\"flex items-center gap-2\">\r\r\n                    <RefreshCw className=\"text-primary size-4\" />\r\r\n                    <span className=\"text-sm font-medium\">Monthly Syncs</span>\r\r\n                  </div>\r\r\n                  <span className=\"text-sm\">143 / 200</span>\r\r\n                </div>\r\r\n                <Progress value={71.5} className=\"h-2\" />\r\r\n              </div>\r\r\n            </div>\r\r\n          </CardContent>\r\r\n        </Card>\r\r\n\r\r\n        {/* Payment Method */}\r\r\n        <Card className=\"mb-8 p-0\">\r\r\n          <CardContent className=\"p-6\">\r\r\n            <div className=\"flex flex-col items-start justify-between gap-4 sm:flex-row\">\r\r\n              <div className=\"space-y-1\">\r\r\n                <h2 className=\"text-lg font-semibold\">Payment Method</h2>\r\r\n                <div className=\"flex items-center gap-2\">\r\r\n                  <CreditCard className=\"text-muted-foreground size-4\" />\r\r\n                  <span className=\"text-muted-foreground text-sm\">\r\r\n                    Visa ending in 4242\r\r\n                  </span>\r\r\n                </div>\r\r\n              </div>\r\r\n              <Button variant=\"outline\">Update Payment Method</Button>\r\r\n            </div>\r\r\n          </CardContent>\r\r\n        </Card>\r\r\n\r\r\n        {/* Billing History */}\r\r\n        <Card className=\"p-0\">\r\r\n          <CardContent className=\"p-6\">\r\r\n            <div className=\"mb-6 flex flex-col items-start justify-between gap-3 sm:flex-row\">\r\r\n              <h2 className=\"text-lg font-semibold\">Billing History</h2>\r\r\n              <Button variant=\"outline\" size=\"sm\">\r\r\n                <Download className=\"mr-2 size-4\" />\r\r\n                Download All\r\r\n              </Button>\r\r\n            </div>\r\r\n\r\r\n            <div className=\"space-y-4\">\r\r\n              {invoices.map((invoice) => (\r\r\n                <div\r\r\n                  key={invoice.id}\r\r\n                  className=\"flex flex-col items-start justify-between gap-3 border-b py-3 last:border-0 sm:flex-row sm:items-center\"\r\r\n                >\r\r\n                  <div className=\"flex items-center gap-3\">\r\r\n                    <div className=\"bg-muted rounded-md p-2\">\r\r\n                      <FileText className=\"text-muted-foreground size-4\" />\r\r\n                    </div>\r\r\n                    <div>\r\r\n                      <p className=\"font-medium\">{invoice.id}</p>\r\r\n                      <p className=\"text-muted-foreground text-sm\">\r\r\n                        {invoice.date}\r\r\n                      </p>\r\r\n                    </div>\r\r\n                  </div>\r\r\n                  <div className=\"flex items-center gap-4\">\r\r\n                    <Badge variant=\"outline\">{invoice.status}</Badge>\r\r\n                    <span className=\"font-medium\">{invoice.amount}</span>\r\r\n                    <Button variant=\"ghost\" size=\"sm\">\r\r\n                      <Download className=\"size-4\" />\r\r\n                    </Button>\r\r\n                  </div>\r\r\n                </div>\r\r\n              ))}\r\r\n            </div>\r\r\n          </CardContent>\r\r\n        </Card>\r\r\n      </div>\r\r\n    </div>\r\r\n  );\r\r\n}\r\r\n",
