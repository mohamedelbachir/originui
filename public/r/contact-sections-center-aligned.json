      "content": "import { Button } from \"@/registry/default/ui/button\";\r\r\nimport { Card, CardContent } from \"@/registry/default/ui/card\";\r\r\nimport { Input } from \"@/registry/default/ui/input\";\r\r\nimport { Label } from \"@/registry/default/ui/label\";\r\r\nimport { Textarea } from \"@/registry/default/ui/textarea\";\r\r\nimport { BrainCircuit, MessageSquare, Wrench } from \"lucide-react\";\r\r\nimport Link from \"next/link\";\r\r\n\r\r\nexport default function CenterAligned() {\r\r\n  return (\r\r\n    <div className=\"container mx-auto px-4 py-24 md:px-6 lg:py-32 2xl:max-w-[1400px]\">\r\r\n      {/* Title */}\r\r\n      <div className=\"mx-auto max-w-xl text-center\">\r\r\n        <h1 className=\"text-3xl font-bold sm:text-4xl\">Contact us</h1>\r\r\n        <p className=\"text-muted-foreground mt-3\">\r\r\n          We&apos;d love to talk about how we can help you.\r\r\n        </p>\r\r\n      </div>\r\r\n\r\r\n      <div className=\"mx-auto mt-12 max-w-lg\">\r\r\n        <Card className=\"p-0\">\r\r\n          <CardContent className=\"p-6\">\r\r\n            <h2 className=\"mb-8 text-xl font-semibold\">Fill in the form</h2>\r\r\n            <form>\r\r\n              <div className=\"grid gap-4 lg:gap-6\">\r\r\n                <div className=\"grid grid-cols-1 gap-4 sm:grid-cols-2 lg:gap-6\">\r\r\n                  <div>\r\r\n                    <Label htmlFor=\"firstname\" className=\"mb-2\">\r\r\n                      First Name\r\r\n                    </Label>\r\r\n                    <Input\r\r\n                      type=\"text\"\r\r\n                      id=\"firstname\"\r\r\n                      placeholder=\"Enter your first name\"\r\r\n                    />\r\r\n                  </div>\r\r\n                  <div>\r\r\n                    <Label htmlFor=\"lastname\" className=\"mb-2\">\r\r\n                      Last Name\r\r\n                    </Label>\r\r\n                    <Input\r\r\n                      type=\"text\"\r\r\n                      id=\"lastname\"\r\r\n                      placeholder=\"Enter your last name\"\r\r\n                    />\r\r\n                  </div>\r\r\n                </div>\r\r\n\r\r\n                <div className=\"grid grid-cols-1 gap-4 sm:grid-cols-2 lg:gap-6\">\r\r\n                  <div>\r\r\n                    <Label htmlFor=\"email\" className=\"mb-2\">\r\r\n                      Email\r\r\n                    </Label>\r\r\n                    <Input\r\r\n                      type=\"email\"\r\r\n                      id=\"email\"\r\r\n                      placeholder=\"Enter your email\"\r\r\n                    />\r\r\n                  </div>\r\r\n                  <div>\r\r\n                    <Label htmlFor=\"phone\" className=\"mb-2\">\r\r\n                      Phone Number\r\r\n                    </Label>\r\r\n                    <Input\r\r\n                      type=\"tel\"\r\r\n                      id=\"phone\"\r\r\n                      placeholder=\"Enter your phone\"\r\r\n                    />\r\r\n                  </div>\r\r\n                </div>\r\r\n\r\r\n                <div>\r\r\n                  <Label htmlFor=\"message\" className=\"mb-2\">\r\r\n                    Details\r\r\n                  </Label>\r\r\n                  <Textarea\r\r\n                    id=\"message\"\r\r\n                    placeholder=\"Tell us about your project\"\r\r\n                    rows={4}\r\r\n                  />\r\r\n                </div>\r\r\n              </div>\r\r\n\r\r\n              <div className=\"mt-6 grid\">\r\r\n                <Button type=\"submit\" size=\"lg\">\r\r\n                  Send inquiry\r\r\n                </Button>\r\r\n              </div>\r\r\n\r\r\n              <div className=\"mt-3 text-center\">\r\r\n                <p className=\"text-muted-foreground text-sm\">\r\r\n                  We&apos;ll get back to you in 1-2 business days.\r\r\n                </p>\r\r\n              </div>\r\r\n     