      "path": "registry/default/blocks/cards-progress-card.tsx",
      "content": "import { Button } from \"@/registry/default/ui/button\";\r\r\nimport {\r\r\n  Card,\r\r\n  CardContent,\r\r\n  CardDescription,\r\r\n  CardFooter,\r\r\n  CardHeader,\r\r\n  CardTitle,\r\r\n} from \"@/registry/default/ui/card\";\r\r\nimport { Progress } from \"@/registry/default/ui/progress\";\r\r\n\r\r\nexport default function ProgressCard() {\r\r\n  return (\r\r\n    <div className=\"max-w-md mx-auto py-6 px-4\">\r\r\n      <Card>\r\r\n        <CardHeader>\r\r\n          <CardTitle>Project Progress</CardTitle>\r\r\n          <CardDescription>Track your project completion</CardDescription>\r\r\n        </CardHeader>\r\r\n        <CardContent className=\"space-y-6\">\r\r\n          <div className=\"space-y-2\">\r\r\n            <div className=\"flex items-center justify-between\">\r\r\n              <span className=\"text-sm font-medium\">Overall Completion</span>\r\r\n              <span className=\"text-sm font-medium\">75%</span>\r\r\n            </div>\r\r\n            <Progress value={75} className=\"h-2\" />\r\r\n          </div>\r\r\n\r\r\n          <div className=\"space-y-4\">\r\r\n            <div className=\"space-y-2\">\r\r\n              <div className=\"flex items-center justify-between\">\r\r\n                <span className=\"text-sm text-muted-foreground\">Frontend</span>\r\r\n                <span className=\"text-sm text-muted-foreground\">90%</span>\r\r\n              </div>\r\r\n              <Progress value={90} className=\"h-1.5\" />\r\r\n            </div>\r\r\n\r\r\n            <div className=\"space-y-2\">\r\r\n              <div className=\"flex items-center justify-between\">\r\r\n                <span className=\"text-sm text-muted-foreground\">Backend</span>\r\r\n                <span className=\"text-sm text-muted-foreground\">65%</span>\r\r\n              </div>\r\r\n              <Progress value={65} className=\"h-1.5\" />\r\r\n            </div>\r\r\n\r\r\n            <div className=\"space-y-2\">\r\r\n              <div className=\"flex items-center justify-between\">\r\r\n                <span className=\"text-sm text-muted-foreground\">Testing</span>\r\r\n                <span className=\"text-sm text-muted-foreground\">40%</span>\r\r\n              </div>\r\r\n              <Progress value={40} className=\"h-1.5\" />\r\r\n            </div>\r\r\n          </div>\r\r\n        </CardContent>\r\r\n        <CardFooter className=\"flex justify-between\">\r\r\n          <Button variant=\"outline\">Details</Button>\r\r\n          <Button>Update</Button>\r\r\n        </CardFooter>\r\r\n      </Card>\r\r\n    </div>\r\r\n  );\r\r\n}\r\r\n",
      "type": "registry:block"
    }
  ],
