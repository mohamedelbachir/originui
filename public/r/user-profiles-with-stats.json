      "content": "import { Avatar, AvatarFallback, AvatarImage } from \"@/registry/default/ui/avatar\";\r\r\nimport { Button } from \"@/registry/default/ui/button\";\r\r\nimport { Card, CardContent } from \"@/registry/default/ui/card\";\r\r\nimport { Badge } from \"@/registry/default/ui/badge\";\r\r\nimport { Edit, Mail, Settings, Star, Users } from \"lucide-react\";\r\r\n\r\r\nexport default function UserProfileWithStats() {\r\r\n  return (\r\r\n    <div className=\"container mx-auto px-4 py-6 md:px-6 2xl:max-w-[1400px]\">\r\r\n      <div className=\"mb-6 flex flex-col items-start justify-between gap-3 sm:flex-row\">\r\r\n        <h1 className=\"text-2xl font-semibold\">Profile</h1>\r\r\n        <div className=\"flex gap-2\">\r\r\n          <Button variant=\"outline\" size=\"sm\">\r\r\n            <Edit className=\"mr-2 size-4\" />\r\r\n            Edit Profile\r\r\n          </Button>\r\r\n          <Button variant=\"outline\" size=\"sm\">\r\r\n            <Settings className=\"mr-2 size-4\" />\r\r\n            Settings\r\r\n          </Button>\r\r\n        </div>\r\r\n      </div>\r\r\n\r\r\n      <div className=\"grid gap-6 md:grid-cols-4\">\r\r\n        {/* Sidebar */}\r\r\n        <div className=\"md:col-span-1\">\r\r\n          <Card className=\"p-0\">\r\r\n            <CardContent className=\"p-6\">\r\r\n              <div className=\"flex flex-col items-center\">\r\r\n                <Avatar className=\"size-20\">\r\r\n                  <AvatarImage\r\r\n                    src=\"https://github.com/shadcn.png\"\r\r\n                    alt=\"User Avatar\"\r\r\n                  />\r\r\n                  <AvatarFallback>JD</AvatarFallback>\r\r\n                </Avatar>\r\r\n                <h2 className=\"mt-4 text-lg font-semibold\">John Doe</h2>\r\r\n                <p className=\"text-muted-foreground text-sm\">\r\r\n                  Product Designer\r\r\n                </p>\r\r\n                <Badge className=\"mt-2\" variant=\"secondary\">\r\r\n                  Pro Member\r\r\n                </Badge>\r\r\n                <Button className=\"mt-4 w-full\" size=\"sm\">\r\r\n                  <Mail className=\"mr-2 size-4\" />\r\r\n                  Message\r\r\n                </Button>\r\r\n              </div>\r\r\n\r\r\n              <div className=\"mt-6 space-y-4\">\r\r\n                <div className=\"flex justify-between text-sm\">\r\r\n                  <span className=\"text-muted-foreground\">Member since</span>\r\r\n                  <span>Jan 2024</span>\r\r\n                </div>\r\r\n                <div className=\"flex justify-between text-sm\">\r\r\n                  <span className=\"text-muted-foreground\">Last active</span>\r\r\n                  <span>2 hours ago</span>\r\r\n                </div>\r\r\n                <div className=\"flex justify-between text-sm\">\r\r\n                  <span className=\"text-muted-foreground\">Role</span>\r\r\n                  <span>Admin</span>\r\r\n                </div>\r\r\n              </div>\r\r\n            </CardContent>\r\r\n          </Card>\r\r\n        </div>\r\r\n\r\r\n        {/* Main Content */}\r\r\n        <div className=\"space-y-6 md:col-span-3\">\r\r\n          {/* Stats */}\r\r\n          <div className=\"grid gap-4 sm:grid-cols-3\">\r\r\n            <Card className=\"p-0\">\r\r\n              <CardContent className=\"p-6\">\r\r\n                <div className=\"flex items-center gap-4\">\r\r\n                  <div className=\"bg-primary/10 rounded-lg p-2\">\r\r\n                    <Star className=\"text-primary size-5\" />\r\r\n                  </div>\r\r\n                  <div>\r\r\n                    <p className=\"text-2xl font-semibold\">128</p>\r\r\n                    <p className=\"text-muted-foreground text-sm\">\r\r\n                      Projects Completed\r\r\n                    </p>\r\r\n                  </div>\r\r\n                </div>\r\r\n              </CardContent>\r\r\n            </Card>\r\r\n            <Card className=\"p-0\">\r\r\n              <CardContent className=\"p-6\">\r\r\n                <div className=\"flex items-center gap-4\">\r\r\n                  <div className=\"bg-primary/10 rounded-lg p-2\">\r\r\n                    <Users className=\"text-primary size-5\" />\r\r\n                  </div>\r\r\n                  <div>\r\r\n                    <p className=\"text-2xl font-semibold\">8.5k</p>\r\r\n                    <p className=\"text-muted-foreground text-sm\">\r\r\n                      Team Members\r\r\n                    </p>\r\r\n                  </div>\r\r\n                </div>\r\r\n              </CardContent>\r\r\n            </Card>\r\r\n            <Card className=\"p-0\">\r\r\n              <CardContent className=\"p-6\">\r\r\n                <div className=\"flex items-center gap-4\">\r\r\n                  <div className=\"bg-primary/10 rounded-lg p-2\">\r\r\n                    <Star className=\"text-primary size-5\" />\r\r\n                  </div>\r\r\n                  <div>\r\r\n                    <p className=\"text-2xl font-semibold\">99%</p>\r\r\n                    <p className=\"text-muted-foreground text-sm\">\r\r\n                      Satisfaction Rate\r\r\n                    </p>\r\r\n                  </div>\r\r\n                </div>\r\r\n              </CardContent>\r\r\n            </Card>\r\r\n          </div>\r\r\n\r\r\n          {/* Recent Activity */}\r\r\n          <Card className=\"p-0\">\r\r\n            <CardContent className=\"p-6\">\r\r\n              <h3 className=\"mb-4 text-lg font-semibold\">Recent Activity</h3>\r\r\n              <div className=\"space-y-4\">\r\r\n                {[1, 2, 3].map((_, i) => (\r\r\n                  <div\r\r\n                    key={i}\r\r\n                    className=\"flex items-start gap-4 border-b pb-4 last:border-0\"\r\r\n                  >\r\r\n                    <div className=\"bg-muted rounded-full p-2\">\r\r\n                      <Star className=\"text-muted-foreground size-4\" />\r\r\n                    </div>\r\r\n                    <div>\r\r\n                      <p className=\"text-sm\">\r\r\n                        Completed project &quot;Dashboard UI&quot;\r\r\n                      </p>\r\r\n                      <p className=\"text-muted-foreground text-xs\">\r\r\n                        2 hours ago\r\r\n                      </p>\r\r\n                    </div>\r\r\n                  </div>\r\r\n                ))}\r\r\n              </div>\r\r\n            </CardContent>\r\r\n          </Card>\r\r\n        </div>\r\r\n      </div>\r\r\n    </div>\r\r\n  );\r\r\n}\r\r\n",
      "type": "registry:block"
    }
