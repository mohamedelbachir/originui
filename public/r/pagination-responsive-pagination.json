      "content": "import {\r\r\n  Pagination,\r\r\n  PaginationContent,\r\r\n  PaginationEllipsis,\r\r\n  PaginationItem,\r\r\n  PaginationLink,\r\r\n  PaginationNext,\r\r\n  PaginationPrevious,\r\r\n} from \"@/registry/default/ui/pagination\";\r\r\nimport { ChevronLeft, ChevronRight } from \"lucide-react\";\r\r\n\r\r\nexport default function ResponsivePagination() {\r\r\n  return (\r\r\n    <div className=\"p-6\">\r\r\n      <Pagination>\r\r\n        <PaginationContent>\r\r\n          {/* Mobile view (icon only) */}\r\r\n          <div className=\"sm:hidden flex items-center gap-2\">\r\r\n            <PaginationItem>\r\r\n              <PaginationLink\r\r\n                href=\"#\"\r\r\n                size=\"icon\"\r\r\n                aria-label=\"Go to previous page\"\r\r\n              >\r\r\n                <ChevronLeft className=\"h-4 w-4\" />\r\r\n              </PaginationLink>\r\r\n            </PaginationItem>\r\r\n            <PaginationItem>\r\r\n              <span className=\"text-sm\">Page 2 of 8</span>\r\r\n            </PaginationItem>\r\r\n            <PaginationItem>\r\r\n              <PaginationLink href=\"#\" size=\"icon\" aria-label=\"Go to next page\">\r\r\n                <ChevronRight className=\"h-4 w-4\" />\r\r\n              </PaginationLink>\r\r\n            </PaginationItem>\r\r\n          </div>\r\r\n\r\r\n          {/* Tablet view (limited pages) */}\r\r\n          <div className=\"hidden sm:flex md:hidden\">\r\r\n            <PaginationItem>\r\r\n              <PaginationPrevious href=\"#\" />\r\r\n            </PaginationItem>\r\r\n            <PaginationItem>\r\r\n              <PaginationLink href=\"#\">1</PaginationLink>\r\r\n            </PaginationItem>\r\r\n            <PaginationItem>\r\r\n              <PaginationLink href=\"#\" isActive>\r\r\n                2\r\r\n              </PaginationLink>\r\r\n            </PaginationItem>\r\r\n            <PaginationItem>\r\r\n              <PaginationLink href=\"#\">3</PaginationLink>\r\r\n            </PaginationItem>\r\r\n            <PaginationItem>\r\r\n              <PaginationEllipsis />\r\r\n            </PaginationItem>\r\r\n            <PaginationItem>\r\r\n              <PaginationLink href=\"#\">8</PaginationLink>\r\r\n            </PaginationItem>\r\r\n            <PaginationItem>\r\r\n              <PaginationNext href=\"#\" />\r\r\n            </PaginationItem>\r\r\n          </div>\r\r\n\r\r\n          {/* Desktop view (full pagination) */}\r\r\n          <div className=\"hidden md:flex\">\r\r\n            <PaginationItem>\r\r\n              <PaginationPrevious href=\"#\" />\r\r\n            </PaginationItem>\r\r\n            <PaginationItem>\r\r\n              <PaginationLink href=\"#\">1</PaginationLink>\r\r\n            </PaginationItem>\r\r\n            <PaginationItem>\r\r\n              <PaginationLink href=\"#\" isActive>\r\r\n                2\r\r\n              </PaginationLink>\r\r\n            </PaginationItem>\r\r\n            <PaginationItem>\r\r\n              <PaginationLink href=\"#\">3</PaginationLink>\r\r\n            </PaginationItem>\r\r\n            <PaginationItem>\r\r\n              <PaginationLink href=\"#\">4</PaginationLink>\r\r\n            </PaginationItem>\r\r\n            <PaginationItem>\r\r\n              <PaginationLink href=\"#\">5</PaginationLink>\r\r\n            </PaginationItem>\r\r\n            <PaginationItem>\r\r\n              <PaginationEllipsis />\r\r\n            </PaginationItem>\r\r\n            <PaginationItem>\r\r\n              <PaginationLink href=\"#\">8</PaginationLink>\r\r\n            </PaginationItem>\r\r\n            <PaginationItem>\r\r\n              <PaginationNext href=\"#\" />\r\r\n            </PaginationItem>\r\r\n          </div>\r\r\n        </PaginationContent>\r\r\n      </Pagination>\r\r\n    </div>\r\r\n  );\r\r\n}\r\r\n",
