{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "user-profiles-developer",
  "type": "registry:block",
  "registryDependencies": [
    "https://ui.woilasoft.com/r/alert.json",
    "https://ui.woilasoft.com/r/badge.json",
    "https://ui.woilasoft.com/r/button.json",
    "https://ui.woilasoft.com/r/card.json"
  ],
  "files": [
    {
      "path": "registry/default/blocks/user-profiles-developer.tsx",
      "content": "import { Button } from \"@/registry/default/ui/button\";\r\r\nimport { Card, CardContent } from \"@/registry/default/ui/card\";\r\r\nimport { Badge } from \"@/registry/default/ui/badge\";\r\r\nimport {\r\r\n  AlertCircle,\r\r\n  Copy,\r\r\n  Key,\r\r\n  RefreshCw,\r\r\n  Settings,\r\r\n  Terminal,\r\r\n  Webhook,\r\r\n} from \"lucide-react\";\r\r\nimport { Alert, AlertDescription, AlertTitle } from \"@/registry/default/ui/alert\";\r\r\n\r\r\nconst apiKeys = [\r\r\n  {\r\r\n    name: \"Production API Key\",\r\r\n    key: \"pk_live_12345678\",\r\r\n    created: \"Mar 1, 2024\",\r\r\n    lastUsed: \"2 hours ago\",\r\r\n  },\r\r\n  {\r\r\n    name: \"Development API Key\",\r\r\n    key: \"pk_test_87654321\",\r\r\n    created: \"Feb 15, 2024\",\r\r\n    lastUsed: \"5 days ago\",\r\r\n  },\r\r\n];\r\r\n\r\r\nconst webhooks = [\r\r\n  {\r\r\n    url: \"https://api.example.com/webhooks/orders\",\r\r\n    events: [\"order.created\", \"order.updated\"],\r\r\n    status: \"Active\",\r\r\n    lastDelivery: \"Successful (2 min ago)\",\r\r\n  },\r\r\n  {\r\r\n    url: \"https://api.example.com/webhooks/users\",\r\r\n    events: [\"user.created\", \"user.deleted\"],\r\r\n    status: \"Failed\",\r\r\n    lastDelivery: \"Failed (10 min ago)\",\r\r\n  },\r\r\n];\r\r\n\r\r\nexport default function DeveloperSettings() {\r\r\n  return (\r\r\n    <div className=\"container mx-auto px-4 py-6 md:px-6 2xl:max-w-[1400px]\">\r\r\n      <div className=\"mx-auto max-w-4xl\">\r\r\n        {/* Header */}\r\r\n        <div className=\"mb-8 flex flex-col items-start justify-between gap-4 sm:flex-row\">\r\r\n          <div>\r\r\n            <h1 className=\"text-2xl font-semibold\">Developer Settings</h1>\r\r\n            <p className=\"text-muted-foreground text-sm\">\r\r\n              Manage your API keys, webhooks, and developer tools\r\r\n            </p>\r\r\n          </div>\r\r\n          <Button variant=\"outline\">\r\r\n            <Terminal className=\"mr-2 size-4\" />\r\r\n            View Documentation\r\r\n          </Button>\r\r\n        </div>\r\r\n\r\r\n        {/* API Keys */}\r\r\n        <Card className=\"mb-8 p-0\">\r\r\n          <CardContent className=\"p-6\">\r\r\n            <div className=\"mb-6 flex flex-col items-start justify-between gap-4 sm:flex-row\">\r\r\n              <div>\r\r\n                <h2 className=\"text-lg font-semibold\">API Keys</h2>\r\r\n                <p className=\"text-muted-foreground text-sm\">\r\r\n                  Manage your API keys for authentication\r\r\n                </p>\r\r\n              </div>\r\r\n              <Button>\r\r\n                <Key className=\"mr-2 size-4\" />\r\r\n                Generate New Key\r\r\n              </Button>\r\r\n            </div>\r\r\n\r\r\n            <div className=\"space-y-4\">\r\r\n              {apiKeys.map((apiKey) => (\r\r\n                <div\r\r\n                  key={apiKey.name}\r\r\n                  className=\"flex flex-col items-start justify-between gap-4 rounded-lg border p-4 sm:flex-row\"\r\r\n                >\r\r\n                  <div className=\"min-w-0 space-y-1\">\r\r\n                    <div className=\"flex items-center gap-2\">\r\r\n                      <h3 className=\"font-medium\">{apiKey.name}</h3>\r\r\n                      <Badge variant=\"secondary\">Active</Badge>\r\r\n                    </div>\r\r\n                    <div className=\"flex items-center gap-2\">\r\r\n                      <code className=\"bg-muted rounded px-2 py-1 text-sm\">\r\r\n                        {apiKey.key}\r\r\n                      </code>\r\r\n                      <Button variant=\"ghost\" size=\"sm\" className=\"h-8\">\r\r\n                        <Copy className=\"size-3\" />\r\r\n                      </Button>\r\r\n                    </div>\r\r\n                    <p className=\"text-muted-foreground text-sm\">\r\r\n                      Created {apiKey.created} â€¢ Last used {apiKey.lastUsed}\r\r\n                    </p>\r\r\n                  </div>\r\r\n                  <div className=\"flex gap-2\">\r\r\n                    <Button variant=\"outline\" size=\"sm\">\r\r\n                      <RefreshCw className=\"mr-2 size-4\" />\r\r\n                      Rotate\r\r\n                    </Button>\r\r\n                    <Button variant=\"destructive\" size=\"sm\">\r\r\n                      Revoke\r\r\n                    </Button>\r\r\n                  </div>\r\r\n                </div>\r\r\n              ))}\r\r\n            </div>\r\r\n          </CardContent>\r\r\n        </Card>\r\r\n\r\r\n        {/* Webhooks */}\r\r\n        <Card className=\"mb-8 p-0\">\r\r\n          <CardContent className=\"p-6\">\r\r\n            <div className=\"mb-6 flex flex-col items-start justify-between gap-4 sm:flex-row\">\r\r\n              <div>\r\r\n                <h2 className=\"text-lg font-semibold\">Webhooks</h2>\r\r\n                <p className=\"text-muted-foreground text-sm\">\r\r\n                  Configure webhook endpoints for real-time updates\r\r\n                </p>\r\r\n              </div>\r\r\n              <Button>\r\r\n                <Webhook className=\"mr-2 size-4\" />\r\r\n                Add Webhook\r\r\n              </Button>\r\r\n            </div>\r\r\n\r\r\n            <div className=\"space-y-4\">\r\r\n              {webhooks.map((webhook) => (\r\r\n                <div\r\r\n                  key={webhook.url}\r\r\n                  className=\"flex flex-col items-start justify-between gap-4 rounded-lg border p-4 sm:flex-row\"\r\r\n                >\r\r\n                  <div className=\"min-w-0 space-y-2\">\r\r\n                    <div className=\"flex flex-wrap items-center gap-2\">\r\r\n                      <h3 className=\"font-medium break-all\">{webhook.url}</h3>\r\r\n                      <Badge\r\r\n                        variant={\r\r\n                          webhook.status === \"Active\"\r\r\n                            ? \"default\"\r\r\n                            : \"destructive\"\r\r\n                        }\r\r\n                      >\r\r\n                        {webhook.status}\r\r\n                      </Badge>\r\r\n                    </div>\r\r\n                    <div className=\"flex flex-wrap gap-1\">\r\r\n                      {webhook.events.map((event) => (\r\r\n                        <Badge key={event} variant=\"secondary\">\r\r\n                          {event}\r\r\n                        </Badge>\r\r\n                      ))}\r\r\n                    </div>\r\r\n                    <p className=\"text-muted-foreground text-sm\">\r\r\n                      Last delivery: {webhook.lastDelivery}\r\r\n                    </p>\r\r\n                  </div>\r\r\n                  <Button variant=\"outline\" size=\"sm\">\r\r\n                    <Settings className=\"mr-2 size-4\" />\r\r\n                    Configure\r\r\n                  </Button>\r\r\n                </div>\r\r\n              ))}\r\r\n            </div>\r\r\n          </CardContent>\r\r\n        </Card>\r\r\n\r\r\n        {/* Security Notice */}\r\r\n        <Alert>\r\r\n          <AlertCircle className=\"size-4\" />\r\r\n          <AlertTitle>Security Notice</AlertTitle>\r\r\n          <AlertDescription>\r\r\n            Keep your API keys secure and never share them publicly. Rotate keys\r\r\n            immediately if they&apos;re compromised.\r\r\n          </AlertDescription>\r\r\n        </Alert>\r\r\n      </div>\r\r\n    </div>\r\r\n  );\r\r\n}\r\r\n",
      "type": "registry:block"
    }
  ],
  "meta": {
    "tags": [
      "user-profiles"
    ]
  }
}