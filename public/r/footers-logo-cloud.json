      "content": "import Link from \"next/link\";\r\r\nimport {\r\r\n  TwitterIcon,\r\r\n  InstagramIcon,\r\r\n  YoutubeIcon,\r\r\n  LinkedinIcon,\r\r\n} from \"lucide-react\";\r\r\n\r\r\nconst footerLinks = [\r\r\n  {\r\r\n    title: \"Product\",\r\r\n    links: [\r\r\n      { title: \"Overview\", href: \"#\" },\r\r\n      { title: \"Features\", href: \"#\" },\r\r\n      { title: \"Solutions\", href: \"#\" },\r\r\n      { title: \"Pricing\", href: \"#\" },\r\r\n    ],\r\r\n  },\r\r\n  {\r\r\n    title: \"Resources\",\r\r\n    links: [\r\r\n      { title: \"Blog\", href: \"#\" },\r\r\n      { title: \"Developers\", href: \"#\" },\r\r\n      { title: \"Support\", href: \"#\" },\r\r\n    ],\r\r\n  },\r\r\n  {\r\r\n    title: \"Company\",\r\r\n    links: [\r\r\n      { title: \"About\", href: \"#\" },\r\r\n      { title: \"Careers\", href: \"#\" },\r\r\n      { title: \"Contact\", href: \"#\" },\r\r\n      { title: \"Partners\", href: \"#\" },\r\r\n    ],\r\r\n  },\r\r\n];\r\r\n\r\r\nconst socialLinks = [\r\r\n  { icon: TwitterIcon, href: \"#\", label: \"Twitter\" },\r\r\n  { icon: InstagramIcon, href: \"#\", label: \"Instagram\" },\r\r\n  { icon: YoutubeIcon, href: \"#\", label: \"YouTube\" },\r\r\n  { icon: LinkedinIcon, href: \"#\", label: \"LinkedIn\" },\r\r\n];\r\r\n\r\r\nconst partners = [\r\r\n  {\r\r\n    name: \"Company 1\",\r\r\n    logo: (props: React.SVGProps<SVGSVGElement>) => (\r\r\n      <svg\r\r\n        {...props}\r\r\n        viewBox=\"0 0 100 40\"\r\r\n        fill=\"none\"\r\r\n        xmlns=\"http://www.w3.org/2000/svg\"\r\r\n      >\r\r\n        <rect width=\"100\" height=\"40\" rx=\"6\" className=\"fill-muted\" />\r\r\n        <text\r\r\n          x=\"50%\"\r\r\n          y=\"50%\"\r\r\n          dominantBaseline=\"middle\"\r\r\n          textAnchor=\"middle\"\r\r\n          className=\"fill-muted-foreground\"\r\r\n          fontSize=\"12\"\r\r\n        >\r\r\n          Logo 1\r\r\n        </text>\r\r\n      </svg>\r\r\n    ),\r\r\n  },\r\r\n  {\r\r\n    name: \"Company 2\",\r\r\n    logo: (props: React.SVGProps<SVGSVGElement>) => (\r\r\n      <svg\r\r\n        {...props}\r\r\n        viewBox=\"0 0 100 40\"\r\r\n        fill=\"none\"\r\r\n        xmlns=\"http://www.w3.org/2000/svg\"\r\r\n      >\r\r\n        <rect width=\"100\" height=\"40\" rx=\"6\" className=\"fill-muted\" />\r\r\n        <text\r\r\n          x=\"50%\"\r\r\n          y=\"50%\"\r\r\n          dominantBaseline=\"middle\"\r\r\n          textAnchor=\"middle\"\r\r\n          className=\"fill-muted-foreground\"\r\r\n          fontSize=\"12\"\r\r\n        >\r\r\n          Logo 2\r\r\n        </text>\r\r\n      </svg>\r\r\n    ),\r\r\n  },\r\r\n  {\r\r\n    name: \"Company 3\",\r\r\n    logo: (props: React.SVGProps<SVGSVGElement>) => (\r\r\n      <svg\r\r\n        {...props}\r\r\n        viewBox=\"0 0 100 40\"\r\r\n        fill=\"none\"\r\r\n        xmlns=\"http://www.w3.org/2000/svg\"\r\r\n      >\r\r\n        <rect width=\"100\" height=\"40\" rx=\"6\" className=\"fill-muted\" />\r\r\n        <text\r\r\n          x=\"50%\"\r\r\n          y=\"50%\"\r\r\n          dominantBaseline=\"middle\"\r\r\n          textAnchor=\"middle\"\r\r\n          className=\"fill-muted-foreground\"\r\r\n          fontSize=\"12\"\r\r\n        >\r\r\n          Logo 3\r\r\n        </text>\r\r\n      </svg>\r\r\n    ),\r\r\n  },\r\r\n  {\r\r\n    name: \"Company 4\",\r\r\n    logo: (props: React.SVGProps<SVGSVGElement>) => (\r\r\n      <svg\r\r\n        {...props}\r\r\n        viewBox=\"0 0 100 40\"\r\r\n        fill=\"none\"\r\r\n        xmlns=\"http://www.w3.org/2000/svg\"\r\r\n      >\r\r\n        <rect width=\"100\" height=\"40\" rx=\"6\" className=\"fill-muted\" />\r\r\n        <text\r\r\n          x=\"50%\"\r\r\n          y=\"50%\"\r\r\n          dominantBaseline=\"middle\"\r\r\n          textAnchor=\"middle\"\r\r\n          className=\"fill-muted-foreground\"\r\r\n          fontSize=\"12\"\r\r\n        >\r\r\n          Logo 4\r\r\n        </text>\r\r\n      </svg>\r\r\n    ),\r\r\n  },\r\r\n  {\r\r\n    name: \"Company 5\",\r\r\n    logo: (props: React.SVGProps<SVGSVGElement>) => (\r\r\n      <svg\r\r\n        {...props}\r\r\n        viewBox=\"0 0 100 40\"\r\r\n        fill=\"none\"\r\r\n        xmlns=\"http://www.w3.org/2000/svg\"\r\r\n      >\r\r\n        <rect width=\"100\" height=\"40\" rx=\"6\" className=\"fill-muted\" />\r\r\n        <text\r\r\n          x=\"50%\"\r\r\n          y=\"50%\"\r\r\n          dominantBaseline=\"middle\"\r\r\n          textAnchor=\"middle\"\r\r\n          className=\"fill-muted-foreground\"\r\r\n          fontSize=\"12\"\r\r\n        >\r\r\n          Logo 5\r\r\n        </text>\r\r\n      </svg>\r\r\n    ),\r\r\n  },\r\r\n  {\r\r\n    name: \"Company 6\",\r\r\n    logo: (props: React.SVGProps<SVGSVGElement>) => (\r\r\n      <svg\r\r\n        {...props}\r\r\n        viewBox=\"0 0 100 40\"\r\r\n        fill=\"none\"\r\r\n        xmlns=\"http://www.w3.org/2000/svg\"\r\r\n      >\r\r\n        <rect width=\"100\" height=\"40\" rx=\"6\" className=\"fill-muted\" />\r\r\n        <text\r\r\n          x=\"50%\"\r\r\n          y=\"50%\"\r\r\n          dominantBaseline=\"middle\"\r\r\n          textAnchor=\"middle\"\r\r\n          className=\"fill-muted-foreground\"\r\r\n          fontSize=\"12\"\r\r\n        >\r\r\n          Logo 6\r\r\n        </text>\r\r\n      </svg>\r\r\n    ),\r\r\n  },\r\r\n];\r\r\n\r\r\nexport default function FooterLogoCloud() {\r\r\n  return (\r\r\n    <footer className=\"w-full border-t bg-background\">\r\r\n      <div className=\"container mx-auto px-4 md:px-6 2xl:max-w-[1400px]\">\r\r\n        {/* Logo Cloud */}\r\r\n        <div className=\"py-12 border-b border-border\">\r\r\n          <div className=\"text-center mb-8\">\r\r\n            <h2 className=\"text-lg font-semibold mb-2\">\r\r\n              Trusted by leading companies\r\r\n            </h2>\r\r\n            <p className=\"text-sm text-muted-foreground\">\r\r\n              Join 50,000+ companies already growing with us\r\r\n            </p>\r\r\n          </div>\r\r\n          <div className=\"grid grid-cols-2 gap-8 md:grid-cols-3 lg:grid-cols-6\">\r\r\n            {partners.map((partner) => (\r\r\n              <Link\r\r\n                key={partner.name}\r\r\n                href=\"#\"\r\r\n                className=\"flex justify-center items-center text-muted-foreground hover:text-foreground transition-colors\"\r\r\n              >\r\r\n                <partner.logo className=\"h-10 w-auto\" />\r\r\n                <span className=\"sr-only\">{partner.name}</span>\r\r\n              </Link>\r\r\n            ))}\r\r\n          </div>\r\r\n        </div>\r\r\n\r\r\n        {/* Footer Content */}\r\r\n        <div className=\"py-12 md:py-16 grid grid-cols-1 md:grid-cols-4 gap-8\">\r\r\n          <div className=\"md:col-span-1\">\r\r\n            <Link href=\"#\" className=\"flex items-center space-x-2\">\r\r\n              <span className=\"font-bold text-xl\">Your Company</span>\r\r\n            </Link>\r\r\n            <p className=\"mt-4 text-sm text-muted-foreground\">\r\r\n              Building the future of digital experiences.\r\r\n            </p>\r\r\n          </div>\r\r\n\r\r\n          {footerLinks.map((group) => (\r\r\n            <div key={group.title}>\r\r\n              <h3 className=\"font-medium\">{group.title}</h3>\r\r\n              <ul className=\"mt-4 space-y-2\">\r\r\n                {group.links.map((link) => (\r\r\n                  <li key={link.title}>\r\r\n                    <Link\r\r\n                      href={link.href}\r\r\n                      className=\"text-sm text-muted-foreground hover:text-primary transition-colors\"\r\r\n                    >\r\r\n                      {link.title}\r\r\n                    </Link>\r\r\n                  </li>\r\r\n                ))}\r\r\n              </ul>\r\r\n            </div>\r\r\n          ))}\r\r\n        </div>\r\r\n\r\r\n        {/* Bottom Bar */}\r\r\n        <div className=\"py-6 border-t border-border flex flex-col md:flex-row justify-between items-center gap-6\">\r\r\n          <p className=\"text-center text-sm text-muted-foreground md:text-left\">\r\r\n            &copy; {new Date().getFullYear()} Your Company, Inc. All rights\r\r\n            reserved.\r\r\n          </p>\r\r\n          <div className=\"flex gap-4\">\r\r\n            {socialLinks.map((link) => (\r\r\n              <Link\r\r\n                key={link.label}\r\r\n                href={link.href}\r\r\n                className=\"text-muted-foreground hover:text-primary transition-colors\"\r\r\n                target=\"_blank\"\r\r\n                rel=\"noreferrer\"\r\r\n                aria-label={link.label}\r\r\n              >\r\r\n                <link.icon className=\"h-5 w-5\" />\r\r\n              </Link>\r\r\n            ))}\r\r\n          </div>\r\r\n        </div>\r\r\n      </div>\r\r\n    </footer>\r\r\n  );\r\r\n}\r\r\n",
      "type": "registry:block"
    }
