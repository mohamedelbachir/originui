{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "contact-sections-with-map",
  "type": "registry:block",
  "registryDependencies": [
    "https://ui.woilasoft.com/r/button.json",
    "https://ui.woilasoft.com/r/card.json",
    "https://ui.woilasoft.com/r/input.json",
    "https://ui.woilasoft.com/r/label.json",
    "https://ui.woilasoft.com/r/textarea.json"
  ],
  "files": [
    {
      "path": "registry/default/blocks/contact-sections-with-map.tsx",
      "content": "import { Button } from \"@/registry/default/ui/button\";\r\r\nimport { Card, CardContent } from \"@/registry/default/ui/card\";\r\r\nimport { Input } from \"@/registry/default/ui/input\";\r\r\nimport { Label } from \"@/registry/default/ui/label\";\r\r\nimport { Textarea } from \"@/registry/default/ui/textarea\";\r\r\nimport { Mail, MapPin, Phone } from \"lucide-react\";\r\r\nimport Link from \"next/link\";\r\r\n\r\r\nexport default function WithMap() {\r\r\n  return (\r\r\n    <div className=\"container mx-auto px-4 py-24 md:px-6 lg:py-32 2xl:max-w-[1400px]\">\r\r\n      <div className=\"mx-auto max-w-2xl lg:max-w-5xl\">\r\r\n        <div className=\"text-center\">\r\r\n          <h1 className=\"text-3xl font-bold sm:text-4xl\">Get in touch</h1>\r\r\n          <p className=\"text-muted-foreground mt-3\">\r\r\n            We&apos;d love to hear from you. Please fill out this form.\r\r\n          </p>\r\r\n        </div>\r\r\n\r\r\n        <div className=\"mt-12 grid gap-8 lg:grid-cols-2\">\r\r\n          {/* Form */}\r\r\n          <Card className=\"p-0\">\r\r\n            <CardContent className=\"p-6\">\r\r\n              <form>\r\r\n                <div className=\"grid gap-4\">\r\r\n                  <div className=\"grid gap-4 sm:grid-cols-2\">\r\r\n                    <div className=\"space-y-2\">\r\r\n                      <Label htmlFor=\"first-name\">First name</Label>\r\r\n                      <Input id=\"first-name\" placeholder=\"First name\" />\r\r\n                    </div>\r\r\n                    <div className=\"space-y-2\">\r\r\n                      <Label htmlFor=\"last-name\">Last name</Label>\r\r\n                      <Input id=\"last-name\" placeholder=\"Last name\" />\r\r\n                    </div>\r\r\n                  </div>\r\r\n\r\r\n                  <div className=\"space-y-2\">\r\r\n                    <Label htmlFor=\"email\">Email</Label>\r\r\n                    <Input id=\"email\" type=\"email\" placeholder=\"Email\" />\r\r\n                  </div>\r\r\n\r\r\n                  <div className=\"space-y-2\">\r\r\n                    <Label htmlFor=\"phone\">Phone</Label>\r\r\n                    <Input id=\"phone\" type=\"tel\" placeholder=\"Phone\" />\r\r\n                  </div>\r\r\n\r\r\n                  <div className=\"space-y-2\">\r\r\n                    <Label htmlFor=\"message\">Message</Label>\r\r\n                    <Textarea\r\r\n                      id=\"message\"\r\r\n                      placeholder=\"Your message\"\r\r\n                      className=\"min-h-[350px]\"\r\r\n                      rows={14}\r\r\n                    />\r\r\n                  </div>\r\r\n\r\r\n                  <Button type=\"submit\">Send message</Button>\r\r\n                </div>\r\r\n              </form>\r\r\n            </CardContent>\r\r\n          </Card>\r\r\n\r\r\n          {/* Map and Contact Info */}\r\r\n          <div className=\"space-y-6\">\r\r\n            <div className=\"bg-muted relative aspect-square w-full overflow-hidden rounded-lg\">\r\r\n              <iframe\r\r\n                src=\"https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2483.9416307263784!2d-0.12574468422944231!3d51.50718397963522!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x487604c7c7eb9be3%3A0x3918653583725b56!2sRiver%20Thames!5e0!3m2!1sen!2suk!4v1647095757714!5m2!1sen!2suk\"\r\r\n                width=\"100%\"\r\r\n                height=\"100%\"\r\r\n                style={{ border: 0 }}\r\r\n                allowFullScreen\r\r\n                loading=\"lazy\"\r\r\n                referrerPolicy=\"no-referrer-when-downgrade\"\r\r\n              ></iframe>\r\r\n            </div>\r\r\n\r\r\n            <div className=\"space-y-4\">\r\r\n              <div className=\"flex items-start gap-4\">\r\r\n                <MapPin className=\"text-muted-foreground mt-1 h-5 w-5 shrink-0\" />\r\r\n                <div>\r\r\n                  <h3 className=\"font-medium\">Visit us</h3>\r\r\n                  <p className=\"text-muted-foreground text-sm\">\r\r\n                    100 Smith Street, Melbourne VIC 3000, Australia\r\r\n                  </p>\r\r\n                </div>\r\r\n              </div>\r\r\n\r\r\n              <div className=\"flex items-start gap-4\">\r\r\n                <Mail className=\"text-muted-foreground mt-1 h-5 w-5 shrink-0\" />\r\r\n                <div>\r\r\n                  <h3 className=\"font-medium\">Email us</h3>\r\r\n                  <Link\r\r\n                    href=\"mailto:hello@example.com\"\r\r\n                    className=\"text-primary text-sm hover:underline\"\r\r\n                  >\r\r\n                    hello@example.com\r\r\n                  </Link>\r\r\n                </div>\r\r\n              </div>\r\r\n\r\r\n              <div className=\"flex items-start gap-4\">\r\r\n                <Phone className=\"text-muted-foreground mt-1 h-5 w-5 shrink-0\" />\r\r\n                <div>\r\r\n                  <h3 className=\"font-medium\">Call us</h3>\r\r\n                  <Link\r\r\n                    href=\"tel:+1234567890\"\r\r\n                    className=\"text-primary text-sm hover:underline\"\r\r\n                  >\r\r\n                    +1 (234) 567-890\r\r\n                  </Link>\r\r\n                </div>\r\r\n              </div>\r\r\n            </div>\r\r\n          </div>\r\r\n        </div>\r\r\n      </div>\r\r\n    </div>\r\r\n  );\r\r\n}\r\r\n",
      "type": "registry:block"
    }
  ],
  "meta": {
    "tags": [
      "contact-sections"
    ]
  }
}