{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "form-layouts-two-column",
  "type": "registry:block",
  "registryDependencies": [
    "https://ui.woilasoft.com/r/button.json",
    "https://ui.woilasoft.com/r/card.json",
    "https://ui.woilasoft.com/r/input.json",
    "https://ui.woilasoft.com/r/label.json",
    "https://ui.woilasoft.com/r/separator.json",
    "https://ui.woilasoft.com/r/textarea.json"
  ],
  "files": [
    {
      "path": "registry/default/blocks/form-layouts-two-column.tsx",
      "content": "import { Button } from \"@/registry/default/ui/button\";\r\r\nimport { Card, CardContent, CardHeader, CardTitle } from \"@/registry/default/ui/card\";\r\r\nimport { Input } from \"@/registry/default/ui/input\";\r\r\nimport { Label } from \"@/registry/default/ui/label\";\r\r\nimport { Separator } from \"@/registry/default/ui/separator\";\r\r\nimport { Textarea } from \"@/registry/default/ui/textarea\";\r\r\n\r\r\nexport default function TwoColumnForm() {\r\r\n  return (\r\r\n    <div className=\"container mx-auto px-4 md:px-6 2xl:max-w-[1400px] py-10\">\r\r\n      <Card>\r\r\n        <CardHeader>\r\r\n          <CardTitle>Profile Settings</CardTitle>\r\r\n        </CardHeader>\r\r\n        <CardContent>\r\r\n          <form>\r\r\n            <div className=\"space-y-8\">\r\r\n              {/* Personal Information */}\r\r\n              <div>\r\r\n                <h3 className=\"text-lg font-medium\">Personal Information</h3>\r\r\n                <p className=\"text-sm text-muted-foreground\">\r\r\n                  Update your personal details here.\r\r\n                </p>\r\r\n\r\r\n                <div className=\"mt-6 grid grid-cols-1 gap-x-6 gap-y-6 sm:grid-cols-2\">\r\r\n                  <div className=\"space-y-2\">\r\r\n                    <Label htmlFor=\"first-name\">First name</Label>\r\r\n                    <Input id=\"first-name\" placeholder=\"Enter first name\" />\r\r\n                  </div>\r\r\n\r\r\n                  <div className=\"space-y-2\">\r\r\n                    <Label htmlFor=\"last-name\">Last name</Label>\r\r\n                    <Input id=\"last-name\" placeholder=\"Enter last name\" />\r\r\n                  </div>\r\r\n\r\r\n                  <div className=\"space-y-2\">\r\r\n                    <Label htmlFor=\"email\">Email</Label>\r\r\n                    <Input id=\"email\" type=\"email\" placeholder=\"Enter email\" />\r\r\n                  </div>\r\r\n\r\r\n                  <div className=\"space-y-2\">\r\r\n                    <Label htmlFor=\"phone\">Phone number</Label>\r\r\n                    <Input\r\r\n                      id=\"phone\"\r\r\n                      type=\"tel\"\r\r\n                      placeholder=\"Enter phone number\"\r\r\n                    />\r\r\n                  </div>\r\r\n\r\r\n                  <div className=\"col-span-full space-y-2\">\r\r\n                    <Label htmlFor=\"about\">About</Label>\r\r\n                    <Textarea\r\r\n                      id=\"about\"\r\r\n                      placeholder=\"Write a few sentences about yourself\"\r\r\n                    />\r\r\n                  </div>\r\r\n                </div>\r\r\n              </div>\r\r\n\r\r\n              <Separator />\r\r\n\r\r\n              {/* Address Information */}\r\r\n              <div>\r\r\n                <h3 className=\"text-lg font-medium\">Address Information</h3>\r\r\n                <p className=\"text-sm text-muted-foreground\">\r\r\n                  Update your shipping address.\r\r\n                </p>\r\r\n\r\r\n                <div className=\"mt-6 grid grid-cols-1 gap-x-6 gap-y-6 sm:grid-cols-2\">\r\r\n                  <div className=\"col-span-full space-y-2\">\r\r\n                    <Label htmlFor=\"street\">Street address</Label>\r\r\n                    <Input id=\"street\" placeholder=\"Enter street address\" />\r\r\n                  </div>\r\r\n\r\r\n                  <div className=\"space-y-2\">\r\r\n                    <Label htmlFor=\"city\">City</Label>\r\r\n                    <Input id=\"city\" placeholder=\"Enter city\" />\r\r\n                  </div>\r\r\n\r\r\n                  <div className=\"space-y-2\">\r\r\n                    <Label htmlFor=\"state\">State / Province</Label>\r\r\n                    <Input id=\"state\" placeholder=\"Enter state\" />\r\r\n                  </div>\r\r\n\r\r\n                  <div className=\"space-y-2\">\r\r\n                    <Label htmlFor=\"postal-code\">ZIP / Postal code</Label>\r\r\n                    <Input id=\"postal-code\" placeholder=\"Enter postal code\" />\r\r\n                  </div>\r\r\n\r\r\n                  <div className=\"space-y-2\">\r\r\n                    <Label htmlFor=\"country\">Country</Label>\r\r\n                    <Input id=\"country\" placeholder=\"Enter country\" />\r\r\n                  </div>\r\r\n                </div>\r\r\n              </div>\r\r\n\r\r\n              <div className=\"flex justify-end gap-4\">\r\r\n                <Button variant=\"outline\">Cancel</Button>\r\r\n                <Button>Save Changes</Button>\r\r\n              </div>\r\r\n            </div>\r\r\n          </form>\r\r\n        </CardContent>\r\r\n      </Card>\r\r\n    </div>\r\r\n  );\r\r\n}\r\r\n",
      "type": "registry:block"
    }
  ],
  "meta": {
    "tags": [
      "form-layouts"
    ]
  }
}