      "content": "import { Button } from \"@/registry/default/ui/button\";\r\r\nimport {\r\r\n  Card,\r\r\n  CardContent,\r\r\n  CardDescription,\r\r\n  CardFooter,\r\r\n  CardHeader,\r\r\n  CardTitle,\r\r\n} from \"@/registry/default/ui/card\";\r\r\n\r\r\nexport default function GradientBorderCard() {\r\r\n  return (\r\r\n    <div className=\"max-w-md mx-auto py-6 px-4\">\r\r\n      <div className=\"relative group\">\r\r\n        <div className=\"absolute -inset-0.5 bg-gradient-to-r from-pink-600 to-purple-600 rounded-lg blur opacity-30 group-hover:opacity-100 transition duration-1000 group-hover:duration-200\"></div>\r\r\n        <Card className=\"relative\">\r\r\n          <CardHeader>\r\r\n            <CardTitle>Create project</CardTitle>\r\r\n            <CardDescription>\r\r\n              Deploy your new project in one-click.\r\r\n            </CardDescription>\r\r\n          </CardHeader>\r\r\n          <CardContent>\r\r\n            <p className=\"text-sm text-muted-foreground\">\r\r\n              Your new project will be created with the latest version of our\r\r\n              framework, complete with authentication, database, and API routes.\r\r\n            </p>\r\r\n          </CardContent>\r\r\n          <CardFooter className=\"flex justify-between\">\r\r\n            <Button variant=\"outline\">Cancel</Button>\r\r\n            <Button>Deploy</Button>\r\r\n          </CardFooter>\r\r\n        </Card>\r\r\n      </div>\r\r\n    </div>\r\r\n  );\r\r\n}\r\r\n",
